<owl:NamedIndividual rdf:about="#Tool_t15_torx_screwdriver_and_socket_wrench">
  <rdf:type rdf:resource="#Tool"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">t15 torx screwdriver and socket wrench</rdfs:label>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">/Item/T15_Torx_Screwdriver_and_Socket_Wrench</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_125326">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the frame up and separate it from the motor.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_125327">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t15_torx_screwdriver_and_socket_wrench"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Turn the part over to the other side. Use the socket wrench to take off the aluminum plate.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_126307">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the two Phillips head screws holding the cover to the motor. After removing the cover to the motor, you can replace the motor.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_59907">
  <rdf:type rdf:resource="#Guide"/>
