<owl:NamedIndividual rdf:about="#Step_162616">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the D35 onto its back.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162617">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the 9 screws with a PH2 Phillips screwdriver on the underside of the D35. The screws are: 7 x 14mm screws 2 x 10mm screws</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163576">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use spudger or plastic opening tool to separate the Deebot backside</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163577">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Carefully lift the back cover from the body of the Deebot</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163579">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">There are 5 cables that need to be carefully disconnected before completely disconnecting the back cover: The 5 cables two blue cables on the front left and right side (located next to the sensor). Represented by the red arrows. Multicolor ribbon in the front center. Represented by the blue arrow. Two (black and red) two prong adapter connected to the motherboard. Represented by the green arrow.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163580">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the 3 screws on the motherboard: 2 x 14mm screws 1 x 10mm screws</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163581">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the screws from the motherboard with a PH2 Phillips Head Screwdriver. The screws are: 2 x 14mm screws 1 x 10mm screws</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163583">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate and disconnect 12 cables bundles connected on the motherboard</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163585">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Carefully disconnect the motherboard from the Deebot body.</step_text>
</owl:NamedIndividual>

