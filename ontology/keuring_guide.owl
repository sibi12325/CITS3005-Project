  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Shark Cordless Pet Perfect Rear Motor Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Shark Cordless Pet Perfect</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">80530</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Shark+Cordless+Pet+Perfect+Rear+Motor+Replacement/80530</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_159655">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the Dust Cup Container from the motor housing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_159663">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the five 14mm screws with the PH2 Bit from the motor housing and pull the housing apart.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_160804">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the motor upward and away from the the housing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_160807">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_soldering_station"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">With the soldering tool separate the motor from the gray wire and the black wire. Turn the motor to the other side. Then separate the motor from the two red wires, and the white wire.</step_text>
</owl:NamedIndividual>

