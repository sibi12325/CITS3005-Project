  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Kenwood Chef A701A New Feet Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Kenwood Chef A701A</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10258</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Kenwood+Chef+A701A+New+Feet+Replacement/10258</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Tool_needlenose_pliers">
  <rdf:type rdf:resource="#Tool"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">needlenose pliers</rdfs:label>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ifixit.com/Tools/Large-Needle-Nose-Pliers/IF145-024</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Tool_2.5mm_flathead_screwdriver">
  <rdf:type rdf:resource="#Tool"/>
