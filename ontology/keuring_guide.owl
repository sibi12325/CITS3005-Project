  <has_step rdf:resource="#Step_71989"/>
  <has_step rdf:resource="#Step_72184"/>
  <has_step rdf:resource="#Step_72186"/>
  <has_step rdf:resource="#Step_72191"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">KitchenAid KSB5CR Power Cable Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">KitchenAid KSB5CR</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">30875</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/KitchenAid+KSB5CR+Power+Cable+Replacement/30875</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_72184">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift up on the power cable to pop it out of its holder.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_72186">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_thin_knife"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Be gentle with the knife or you will break the circuit board. Slide the knife into the metal loop holding the insulation before the white copper wire, and wiggle it back and forth until it loosens. Pull the wire out from the metal loop that you loosened. Repeat this process with the black copper wire.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_72191">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the Phillips #2 screwdriver, remove the #8 Phillips 6mm screw to which the green wire is attached.</step_text>
</owl:NamedIndividual>

