  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cut zip tie holding the power cable together using handheld wire cutters.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_130314">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_solder"/>
