  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Honeywell HY-204 Rotation Button Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Honeywell HY-204</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">32061</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Honeywell+HY-204+Rotation+Button+Replacement/32061</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_74679">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_ph0_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the four 14 mm screws  from the back of the device using the Phillips #0 Screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_74682">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the front and back parts of the case away from each other until the back is free from the front. This may require a large amount of force.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_74723">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_ph0_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 3mm screws found just below the top of the device using the Phillips #0 Screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_74724">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pry off the control panel by forcefully lifting the tab located inside the front of the fan with the plastic opening tool.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_74707">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_ph0_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 10mm Phillips #0 screws that are located next to the rotation button.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_74727">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Peel back the heat shrink tubing on both wires connecting to the button.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_74728">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_soldering_station"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'''Caution''' The soldering iron is very hot, be careful to avoid burns. Desolder the two wires connected to the button and remove the button.</step_text>
</owl:NamedIndividual>

