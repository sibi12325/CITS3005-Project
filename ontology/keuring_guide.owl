</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_40096">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Make sure the door is closed before you start repairing the spring. If the spring is detached reinsert it back into the bracket.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41159">
  <rdf:type rdf:resource="#Step"/>
