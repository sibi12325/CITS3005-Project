<owl:NamedIndividual rdf:about="#Step_143363">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">To start the disassembly of the Magic Bullet NutriBullet Pro you will begin from the bottom. Flip the device over to the bottom. Unscrew the four 1 cm screws using #2 Phillips Head Screwdriver. Remove the loose bottom portion of the device by pulling up from the bottom.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_143365">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the two 1 cm screws holding down the power cord using the #2 Phillips Head Screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_143687">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the three 1 cm screws on the device circled on the screws pictures using the #1 Philips Head Screwdriver. Lift off the grey plastic piece off. Slide off the silver metal piece.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_143690">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the device over. Remove the three rubber seals covering the screws by pulling the components upward with the tweezer. Unscrew the three 16 mm screws using the #2 Phillips Head Screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_143693">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull off the casing from the device.</step_text>
</owl:NamedIndividual>

