</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163490">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull heating element out of the way.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163493">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Twist warming plate clockwise so that the tab is out of place. Remove warming plate.</step_text>
</owl:NamedIndividual>

