  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ensure device is turned off, unplugged, cooled down and clean before attempting any replacements. Start by unscrewing the triple-square bolt.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_132829">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
