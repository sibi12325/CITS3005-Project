<owl:NamedIndividual rdf:about="#Step_74201">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_putty_knife"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a butter knife or putty knife to remove grease from around the gasket. Place grease in motor housing cover.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_74202">
