  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the two metal spudgers, pry the middle control knob directly outward.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_114219">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
