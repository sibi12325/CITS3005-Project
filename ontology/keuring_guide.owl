  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ninja Professional Blender BL660 Fan Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ninja Professional Blender BL660</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">99022</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Ninja+Professional+Blender+BL660+Fan+Replacement/99022</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_183584">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_needlenose_pliers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a wrench or a pair of pliers to help remove the nut on top of the fan. Holding the fan itself will help make the process easier. The fan blades are sharp. Handle with care. Using a paper towel or cloth while handling is recommended.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_183585">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the plastic opening tool to loosen the washer.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_183586">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove washer then slide the fan off the screw. Replace the fan as needed. The fan has a keyed hole, thus it fits on the shaft in a specific way. Ensure you put it on correctly.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_99026">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <uses_tool rdf:resource="#Tool_soldering_station"/>
