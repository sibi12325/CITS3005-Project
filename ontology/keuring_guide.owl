</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_193733">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the 6 screws along the bottom of the rear casing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_193734">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the plastic cover over the circuit board by pressing the lever on the side. A pry tool may be required.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_193735">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the fuse with a plastic pry tool.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_103620">
  <rdf:type rdf:resource="#Guide"/>
