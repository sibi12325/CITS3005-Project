  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the springs. There is a top spring and bottom spring.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_164029">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use needle nose tweezers to free one end of the spring. Turn the spring until it is completely free.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_164030">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove spring for replacement. Repeat steps for bottom spring.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_86998">
  <rdf:type rdf:resource="#Guide"/>
