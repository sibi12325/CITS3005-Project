  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hamilton Beach 51101BA Motor Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hamilton Beach 51101BA</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">52171</has_guidid>
