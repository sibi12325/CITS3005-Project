  <has_step rdf:resource="#Step_165291"/>
  <has_step rdf:resource="#Step_165292"/>
  <has_step rdf:resource="#Step_165295"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">RYDIS H68 Pro Side Brush Motor Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">RYDIS H68 Pro</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">87043</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/RYDIS+H68+Pro+Side+Brush+Motor+Replacement/87043</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165287">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Begin by unscrewing the 2 10mm screws using a Phillips #1 screwdriver. Unlatch the latches indicated. It can be easily done with your finger. Once unsecured, the covering for the sensors can easily be removed allowing access to the sensors. The reason for these first few steps is to avoid damaging the signal wires for the sensor.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165291">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This step just shows you how everything looks after the previous steps were completed. There are sensors on both the left and right side. This can be done for either side.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165292">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the five 10mm screws using a Phillips #1 screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165295">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The side brush motor can now be removed by lifting it up.</step_text>
</owl:NamedIndividual>

