<owl:NamedIndividual rdf:about="#Step_209616">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the top panel by bending the tabs on each side straight up.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_209617">
  <rdf:type rdf:resource="#Step"/>
