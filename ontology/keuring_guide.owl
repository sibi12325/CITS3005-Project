</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165226">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">21</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Detach the hose from both sides of the carbonation system. You will have to use force to detach this piece.</step_text>
</owl:NamedIndividual>

