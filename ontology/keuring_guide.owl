<owl:NamedIndividual rdf:about="#Step_74306">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the three 60mm Phillips size 0 screws holding the top cover on.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_74307">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the top cap of the machine by turning it in a counter-clockwise motion.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_77530">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the grey plastic ring from the rim of the filter basket holder.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_77531">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift and remove the top black plastic cover from the machine.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_74090">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a Phillip size 2 screw driver to remove the four 15mm screws holding the pump to the machine's base. Lift the water heater up and remove it from the base.</step_text>
</owl:NamedIndividual>

