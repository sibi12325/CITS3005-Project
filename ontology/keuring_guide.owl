  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ninja Kitchen System Pulse</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">56404</has_guidid>
