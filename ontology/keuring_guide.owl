<owl:NamedIndividual rdf:about="#Step_185249">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the prying tools to open the four plastic screw holes on the bottom of the blender.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185253">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the Phillips #1 Screwdriver to unscrew the four 16 mm screws and remove the plastic bottom.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185257">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gently remove the circuit board.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185260">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the spudger to detach the black wire from the circuit board. Repeat for yellow wire.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185264">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
