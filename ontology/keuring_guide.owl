  <has_step rdf:resource="#Step_144616"/>
  <has_step rdf:resource="#Step_144618"/>
  <has_step rdf:resource="#Step_144619"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Keurig K70 Top  Needle Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Keurig K70</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">72141</has_guidid>
