  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Shark Rotator Powered Lift-Away</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">98553</has_guidid>
