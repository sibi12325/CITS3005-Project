</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_114213">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Move the motor platform to the side. Lift up the motor and remove it from the blender.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_114216">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_soldering_station"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Some soldering experience required! Desolder the red wire from the green plastic piece.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_114217">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cut the thin black wire from the wire cap.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_52173">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_basic_pliers"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <uses_tool rdf:resource="#Tool_magnetic_project_mat"/>
