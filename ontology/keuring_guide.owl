  <has_step rdf:resource="#Step_130665"/>
  <has_step rdf:resource="#Step_130666"/>
  <has_step rdf:resource="#Step_130667"/>
  <has_step rdf:resource="#Step_130670"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">AmazonBasics B00Q3KFX8U Drive Gear Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">AmazonBasics B00Q3KFX8U</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">62156</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/AmazonBasics+B00Q3KFX8U+Drive+Gear+Replacement/62156</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_130658">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the shredder assembly from the paper scrap bin.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_130662">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 10mm PH1 screws holding the small black panel in place.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_130664">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the six 14mm PH1 screws holding the bottom cover in place. Remove the cover by lifting straight upwards. One of the screws will be hidden beneath a warranty sticker. Simply push the head of the screwdriver through the sticker to access the screw.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_130665">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the blade/motor assembly from the top cover by lifting straight upwards. Do not disconnect any wiring just yet. The blades are sharp. Coming into contact with them may result in injury.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_130666">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the five 8mm PH2 screws from the bracket holding the gears in place.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_130667">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a metal spudger to pry the bracket from the main shredder assembly. If the bracket is attached too tightly for the spudger, carefully use a metal screwdriver to pry it loose.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_130670">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two plastic drive gears from the shredder assembly by lifting straight upwards.</step_text>
</owl:NamedIndividual>

