  <uses_tool rdf:resource="#Tool_masking_tape"/>
  <uses_tool rdf:resource="#Tool_digital_multimeter"/>
  <has_step rdf:resource="#Step_71218"/>
  <has_step rdf:resource="#Step_71219"/>
  <has_step rdf:resource="#Step_71220"/>
  <has_step rdf:resource="#Step_71221"/>
  <has_step rdf:resource="#Step_71222"/>
  <has_step rdf:resource="#Step_71223"/>
