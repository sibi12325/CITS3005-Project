</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181400">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the roller brush from the plastic frame using your hands by lifting it away.</step_text>
</owl:NamedIndividual>

