  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mr. Coffee DW13</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">22648</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Mr.+Coffee+DW13+Heating+Plate+Replacement/22648</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59222">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the Phillips Head #2 screwdriver, remove the two 13 mm screws that secure the silver support bracket.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59239">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gently lift the silver support bracket to free the heating plate from the heating coil.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59223">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_needlenose_pliers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pinch and loosen the crimps with the needle-nose pliers.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59224">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Once the crimps are loosened, slide them down the tubing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59225">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Separate the tubing from the heating coil by grasping the tubing in one hand and the heating coil in the other. Gently pull them apart.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59226">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place your hand on the front side of the heating plate and push towards the bottom of the coffee maker to pop the heating plate out of place.</step_text>
</owl:NamedIndividual>

