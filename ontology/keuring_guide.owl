<owl:NamedIndividual rdf:about="#Step_133162">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ensure Keurig is powered off and unplugged. Turn the Keurig over, and unscrew five 25.5 mm Phillip #1 screws, and remove bottom panel.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_133163">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_wire_cutter"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unwind the Power Cord from the wire channel</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_133164">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_wire_cutter"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Make three cuts along the wires shown Solder the Power Cord Replacement to the same wire attachments</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_63123">
