<owl:NamedIndividual rdf:about="#Step_181567">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Make sure the vacuum is unplugged. Warning: Avoid pressing the red trigger because it will turn on and begin suction.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_184759">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the purple button at the top of the device.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185743">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Separate the device with the purple button.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181568">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the dust bin's red release lever on the side of the device. Make sure if the dust bin is full you open the vacuum over a trash can.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185744">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Push the red lever to release the dust bin lid.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181570">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the red lever below the vacuum.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185745">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Push the red lever,  pull the back up and away from the dust bin to separate them.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_182097">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate two screws at the back of the attachment holder.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185467">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t8_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a T8-Torque screwdriver to take out the two 26mm screws out.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_182098">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Loosen the attachment holder by using a metal spudger.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185748">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the attachment holder.</step_text>
</owl:NamedIndividual>

