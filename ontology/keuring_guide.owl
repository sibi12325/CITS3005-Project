</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_164213">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove three 10mm Philips #1 screws that are securing the wheel assembly.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165335">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the 10mm Phillips #1 screw securing the spring. Then undo the spring from the plastic hook to completely remove it from the wheel assembly.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165340">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift up on the plastic piece to remove the entire wheel assembly.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165349">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the plastic frame by pulling outward on the two spots where it is fastened.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165357">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the four 10mm Phillips #1 screws around the edges of the cover.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165361">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift up on the top cover to remove it. Removing the top cover reveals the gear system consisting of three gears.</step_text>
</owl:NamedIndividual>

