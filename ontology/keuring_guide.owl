</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_129966">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the handle's cap carefully. Watch out for the quick burst button, it likes to run away.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_129969">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Slide the speed switch forward and lift upward off of the handle.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_129972">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_wire_cutter"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove speed level from the inside of the case by applying light pressure toward you. Do not pull on the wires to remove the speed level.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_129973">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the beater release button from speed level by removing the two 1.3 mm Philips #2 screws. Replace the speed level assembly.</step_text>
</owl:NamedIndividual>

