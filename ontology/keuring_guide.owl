  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">KitchenAid KSB5CR Ribbon Cable Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">KitchenAid KSB5CR</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">30753</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/KitchenAid+KSB5CR+Ribbon+Cable+Replacement/30753</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_71992">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two #6 Hex 82mm screws using the Phillips #2 screwdriver. Remove the rubber O-ring by lifting it up.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_71993">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the ribbon cable from the chassis by lifting it up. Remove the other end of the ribbon cable from the circuit board by lifting it up.</step_text>
</owl:NamedIndividual>

