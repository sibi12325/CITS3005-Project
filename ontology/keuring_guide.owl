</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_199351">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 10mm Phillips #2  screws on the back of the green printed circuit board. If the circuit board needs to be replaced, locate where the two white wires are soldered to the board, and de-solder the connection.</step_text>
</owl:NamedIndividual>

