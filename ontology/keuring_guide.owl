<owl:NamedIndividual rdf:about="#Step_100746">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Die Aufschumdse abschrauben. Dafr kann ein Englnder verwendet werden.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100747">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Den Kaffeebohnenbehlter entriegeln und abnehmen.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100748">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Die fnf PH1 Schrauben an der linken und rechten Seite lsen. Achtung 220 Volt! Unbedingt Stecker vorher rausziehen! Maschine vor dem ffnen abkhlen lassen!</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100749">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Den rechten Schalter durch lsen der Schaltermutter ausbauen. Mit einer kleinen Zange die Kontermutter festhalten. Achtung: Dabei nicht das Gehuse verkratzen!</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100750">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Das Umschaltventil sitzt von oben gesehen an der rechten Gehusewand. Mutter zur Brhgruppe ffnen. Falls sie festsitzt mit Essigtinktur behandeln und einwirken lassen. Schlauch zum Siebtrger abschrauben. Ablaufschlauch abziehen.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100751">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t25_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Die Befestigungsschraube mit einem T25 Torxschraubendreher entfernen.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100753">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Die Halterung vom Umschlaltventil lsen.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100755">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Die Mutter fr den Ablaufschlauch lsen Die Dichtung in der Mutter prfen und ggf. ersetzen. Dichtung mit Siliconfett einfetten.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100754">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Durch das Drehen der Dampfdse wird das Ventil bettigt. Je nach Stellung der Dse wird das Wasser bzw. der Dampf durch die Dse oder zum Siebtrger geleitet.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100756">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dampfdse abschrauben. Dichtungen an der Dampfdse prfen und fetten. Beim Einbau darauf achten, dass der Stift am Ende der Dse in die Rille der Ventilspindel greift. Funktionsweise des Umschaltventils durch Drehen der Dse prfen.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100757">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Die Ventilspindel kann jetzt seitlich aus dem Ventil gezogen werden.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100758">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">11</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dichtungen berprfen und ggf. ersetzen. Dichtungen vor dem Einbau mit Siliconfett einfetten.</step_text>
</owl:NamedIndividual>

