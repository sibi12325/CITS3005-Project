</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_40185">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">13</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two Phillips #4 1 cm screws that were under the faceplate.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_40190">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">14</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Forcefully bend back the faceplate in order to get to the knob.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_40191">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">15</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove pin holding the On/Off switch in place.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_40192">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">16</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove switch from the opening it protrudes from. This will take a little bit of manipulating but it will come out.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_11270">
  <rdf:type rdf:resource="#Guide"/>
