<owl:NamedIndividual rdf:about="#Step_181031">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Make sure the vacuum is unplugged from charger. Warning: Avoid pressing red trigger because the vacuum will turn on the device and begin suction.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181032">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the purple button at the top of the device.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185746">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Separate the device with the purple button.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185747">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Take out the filter.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181033">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate and remove the 10mm Phillips head screw using a Phillips #1 screw driver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181034">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the battery pack down and away from the main body and remove it from the device.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181573">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Take out the 10mm screw using a Phillips #1 screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181574">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a spudger to separate the back plate covering the motor. Be careful when trying to remove the motor with a metal spudger.  Be sure your battery is removed before trying to fully remove the motor.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181575">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a flat spudger to loosen and remove the motor out of the vacuum.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_98532">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_t8_torx_screwdriver"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <has_step rdf:resource="#Step_181567"/>
