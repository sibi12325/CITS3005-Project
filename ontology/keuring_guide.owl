<owl:NamedIndividual rdf:about="#Step_113923">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">18</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hold the black panel in front of you with the handle facing the other side. Locate the two hinges that will  be pressed in the next step.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_113926">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">19</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rotate the black panel so that the handle is now facing you. Insert your hands through the rectangular holes. Using your index fingers, press the hinges that were located in the previous step.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_114784">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">20</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the black handle for replacement.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_52171">
