  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the Phillips #2 screwdriver to remove one 9.53-mm screw.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_55007">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">15</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the white latch by unhooking it from the spring wire and gently lifting straight up.</step_text>
</owl:NamedIndividual>

