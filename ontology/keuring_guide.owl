  <has_step rdf:resource="#Step_73471"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">KitchenAid Classic Mixer K45SSWH Circuit Board Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">KitchenAid Classic Mixer K45SSWH</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">31481</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/KitchenAid+Classic+Mixer+K45SSWH+Circuit+Board+Replacement/31481</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_73466">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a phillips 2 screwdriver, remove the 10 mm screw from top of back cover. The rear cover is now completely free from the mixer.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_73468">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a phillips 1 screwdriver, remove the 9.5 mm screw holding the circuit board in place. The screw may be hidden underneath the seal.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_73470">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_needlenose_pliers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using pliers, remove the connections for each of the four wires connecting to the circuit board. Remember which color wire corresponds to which connection.  Improper placement will not allow your mixer to turn on.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_73471">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Grab the four wires from the circuit board. Tilt the board back to remove it.</step_text>
</owl:NamedIndividual>

