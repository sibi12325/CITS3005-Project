</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_54775">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">12</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a Phillips #2 screwdriver to remove the three 9.53-mm screws from inside the main body. Next, turn the device upright and lift the upper casing from around the button gently.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_54776">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">13</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the button by simply pulling it up. Don't lose the spring under the button! Without it, the blender will not function.</step_text>
</owl:NamedIndividual>

