<owl:NamedIndividual rdf:about="#Guide_26600">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_cross_screwdriver"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
