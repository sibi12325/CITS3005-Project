</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181136">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the belt, found on the right hand side, from the motor gear.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181137">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift up the brush head from the plastic housing, slight force may be needed. Remove  the band from the brush head.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181138">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the plastic caps on each end of the brush head.</step_text>
</owl:NamedIndividual>

