  <has_step rdf:resource="#Step_58859"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Oster 6329 Circuit Board Resistor Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Oster 6329</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">22533</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Oster+6329+Circuit+Board+Resistor+Replacement/22533</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_58832">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a #1 phillips head screwdriver, remove the four 1cm philips head screws holding the circuit board to the inside of the toaster shell.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_58834">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Grasp the circuit board and pull on it perpendicular to the attached surface.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_58840">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Inspect the circuit board for any burnt or otherwise damaged components. Be sure to carefully identify the damaged components that need replacement. For help identifying resistor values look at [http://en.wikipedia.org/wiki/Electronic_color_code|wikipedia's electronic color code article].</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_58850">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_soldering_station"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the soldering kit to unsolder the damaged component. For soldering Instructions look at the [guide|750|iFixit Soldering Guide].</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_58859">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Once the damaged component has been unsoldered from the circuit board, remove it.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_22535">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_needlenose_pliers"/>
