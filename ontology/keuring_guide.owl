<owl:NamedIndividual rdf:about="#Step_144605">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the handle while pressing the lift away button to remove the body of the vacuum.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_144606">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the base of the vacuum over to access the bottom. Remove the two small wheels with a metal spudger.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_144608">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the nine 16 mm screws using the Phillips #2 Screwdriver. Press on the side tabs in the middle of the panel and pull up to reveal the screw under the hose connection. Remove the two 13.5 mm screws using the Phillips #2 Screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_144610">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull up on the cover and move it to the side. Remove the wire attached to the  base by squeezing the pin and pulling up.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_144612">
  <rdf:type rdf:resource="#Step"/>
