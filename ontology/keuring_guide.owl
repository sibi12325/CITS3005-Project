  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 5mm Phillips #1 screws in the holes closest to the left wheel.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_168371">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">11</step_order>
