  <has_step rdf:resource="#Step_164033"/>
  <has_step rdf:resource="#Step_164036"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Black and Decker TR1278B Circuit Board Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Black and Decker TR1278B</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">86998</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Black+and+Decker+TR1278B+Circuit+Board+Replacement/86998</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_164032">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the circuit board inside front cover.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_164033">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a Phillips head #2 screwdriver to unscrew all five screws that attach the circuit board to the top cover. Remove the circuit board.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_164036">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_soldering_station"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Inspect the circuit board for any burnt or otherwise damaged components. Use a  soldering kit to unsolder the damaged component. Once the damaged component has been unsoldered from the circuit board, remove (and replace) it. [https://www.ifixit.com/Guide/How+To+Solder+and+Desolder+Connections/750|iFixit Soldering Guide.]</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_87042">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
