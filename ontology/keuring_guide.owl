<owl:NamedIndividual rdf:about="#Step_114150">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">15</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pinch the two sides of the top of the coffee press to unlatch. Push down into the device as you pinch. Orient the device with the front facing away from you.</step_text>
</owl:NamedIndividual>

