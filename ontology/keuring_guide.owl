  <has_step rdf:resource="#Step_82437"/>
  <has_step rdf:resource="#Step_82439"/>
  <has_step rdf:resource="#Step_82445"/>
  <has_step rdf:resource="#Step_82450"/>
  <has_step rdf:resource="#Step_82453"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">iRobot Roomba 655 Pet Series Display  Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">iRobot Roomba 655 Pet Series</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">36882</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/iRobot+Roomba+655+Pet+Series+Display++Replacement/36882</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_82450">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the silver tab, and clear tinted display off Remove the 4 - 9mm  screws holding the display with your Phillips head #0</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_82453">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the display Remove the plastic protectant film from the motherboard</step_text>
</owl:NamedIndividual>

