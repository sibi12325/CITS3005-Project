  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">107859</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Neato+Botvac+D8000+Top+Face+Panel+Replacement/107859</has_url>
</owl:NamedIndividual>

