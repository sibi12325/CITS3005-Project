</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_143893">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_ph0_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 8mm Phillips screws from the plastic body.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_143899">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the cord  and cut the black and red wires across the black heat shrink. Be careful when soldering the new power cable.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_72141">
  <rdf:type rdf:resource="#Guide"/>
