  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mr. Coffee BVMC-SJX33GT</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">40935</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Mr.+Coffee+BVMC-SJX33GT+Heating+Plate+Replacement/40935</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_94325">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the two 17mm screws securing the bracing bar. Unscrew the screws with a #2 Phillip's head screwdriver and remove the bracing bar</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_94326">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a finger to keep the heating plate in place, grip the metal tubing and gently pull it away from the plate. Set the metal tubing aside.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_94327">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Push the heating plate up from below and remove.</step_text>
</owl:NamedIndividual>

