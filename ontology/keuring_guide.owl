  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a spudger or a coin to turn the circular lock clockwise to unlock the brush roll. Remove the lock cover.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_187450">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift brush out of the enclosure.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_187451">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_ph0_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove (4) 13.0 mm Phillips #0 head screws and remove the green cover.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_187454">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Take out the motor assembly.</step_text>
</owl:NamedIndividual>

