</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_187320">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Press the bin button and pull the bin out.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_187321">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the Roomba over and unscrew the side brush screw using the Phillips #2 screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_187322">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the (4) four screws to the back plate and lift off.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_187323">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the (4) four screws to the brush cage. Lift the brush cage out.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_187324">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the (3) screws to the left wheel and the right wheel. Lift the wheels out and remove them from the device.</step_text>
</owl:NamedIndividual>

