  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 8mm screws on the bottom of the plunger using a Tri-point Y1 screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_209618">
  <rdf:type rdf:resource="#Step"/>
