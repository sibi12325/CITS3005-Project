</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_136380">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This video shows the tools needed for the Gaggia Classic Repair.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_136381">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This video shows how to prepare the Gaggia Classic for the repair.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_136382">
  <rdf:type rdf:resource="#Step"/>
