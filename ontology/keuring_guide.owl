  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the gasket up with your hands or a spuger and remove it.</step_text>
</owl:NamedIndividual>

