</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_72137">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a #1 Phillips screwdriver bit or a ratchet wrench with a Phillips screwdriver bit to remove screws located on the interior frame. The interior edges of the metal frame are sharp and can cause minor cuts.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_72138">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Plastic sides should be easily removed after all screws are taken out.</step_text>
</owl:NamedIndividual>

