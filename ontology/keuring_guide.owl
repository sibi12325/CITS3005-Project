</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_187157">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the three screws that hold the motor to the top case using a Phillips #1 screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_187161">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull out the metal piece from the casing. Pull apart the plastic casing that contains the rod. Take off the plastic gears.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_187163">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the Phillips #1 screwdriver to take off the screws that hold the white plastic to the metal piece. Pull apart the white plastic from the metal piece.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_187165">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_electric_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use an electric screwdriver with a Phillips #1 head to take off the top metal piece.</step_text>
</owl:NamedIndividual>

