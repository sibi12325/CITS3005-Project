  <has_step rdf:resource="#Step_144328"/>
  <has_step rdf:resource="#Step_144329"/>
  <has_step rdf:resource="#Step_144289"/>
  <has_step rdf:resource="#Step_144292"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">iRobot Roomba 770 Main Wheels Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">iRobot Roomba 770</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">72236</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/iRobot+Roomba+770+Main+Wheels+Replacement/72236</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_144327">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the J1 Philips Screwdriver, remove the 15.5 mm screw located in the center of the side brush. Remove the brush by pulling upwards. Do not lean directly over the brush when attempting to remove. Removing the brush requires a firm pull and may release suddenly.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_144328">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the J1 Philips Screwdriver, remove the two 15.5 mm screws located on the bottom of the plate covering the bottom of the roomba. Lift upwards to remove the bottom plate.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_144329">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the J1 Philips Screwdriver, remove the two gold 15.5 mm screws located on the blue plastic module the side brush had previously been attached too. Lift the blue module upwards. The module contains the motor for the side brush.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_144289">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the three 15.5 mm screws surrounding each wheel. Use a J1 Philips screw driver to remove these. The screw towards the center for each wheel may be hard to locate and may seem inaccessible. The head of your screw is long enough to reach it. Use your J1 Philips screwdriver to remove these screws.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_144292">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull up on the wheels to remove them from the device.</step_text>
</owl:NamedIndividual>

