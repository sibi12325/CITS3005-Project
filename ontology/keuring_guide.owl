  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use Phillips head screwdriver and wrench to remove screw and nut securing the plastic bracket.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41167">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_ph0_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use Phillips head screwdriver and pliers to remove screw and nut securing transformer.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41168">
  <rdf:type rdf:resource="#Step"/>
