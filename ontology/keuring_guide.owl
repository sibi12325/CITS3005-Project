<owl:NamedIndividual rdf:about="#Step_128928">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Be sure to unplug the device from the wall outlet before disassembling. Remove the bottom head of the vacuum cleaner by pressing the push button.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_128929">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Carefully unscrew the 6 screws with a  Phillips screwdriver. Set aside.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_128932">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Separate the top portion of the head from the bottom. Make sure not to pull off or damage the connected wires.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_128933">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Slip off the belt.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_128934">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the brush roll from its slots.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_128936">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_utility_scissors"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use scissors to cut strings and other debris from the brush roll. Avoid cutting the bristles.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_128938">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Return the clean brush roll and re-situate  the belt to secure it in place.</step_text>
</owl:NamedIndividual>

