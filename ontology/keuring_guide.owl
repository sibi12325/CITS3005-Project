  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Black and Decker Toast R Oven Door Spring Mechanism Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Black and Decker Toast R Oven</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10888</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Black+and+Decker+Toast+R+Oven+Door+Spring+Mechanism+Replacement/10888</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_39067">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">13</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the screwdriver to remove the spring from the unit. Place the screwdriver inside the spring loop. Pull to unhinge the spring from the back panel.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_39068">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">14</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the spring from the door mechanism by pulling it off. Replace the old spring with a new spring. When inserting a new spring, make sure that the spring is connected to the lower hole of the back panel (the hole above is used for the back casing).</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_10889">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_digital_multimeter"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <has_step rdf:resource="#Step_39066"/>
  <has_step rdf:resource="#Step_39069"/>
  <has_step rdf:resource="#Step_39073"/>
  <has_step rdf:resource="#Step_39075"/>
