  <has_step rdf:resource="#Step_86798"/>
  <has_step rdf:resource="#Step_86799"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nostalgia Electrics PCM805 Motor Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nostalgia Electrics PCM805</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">38262</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Nostalgia+Electrics+PCM805+Motor+Replacement/38262</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_86798">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the four 12mm Phillips #02 screws to free the motor housing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_86799">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Line up the prongs on the motor shaft with the holes in the heating element and pull the motor out.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_39258">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_7_16_socket"/>
