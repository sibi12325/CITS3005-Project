  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Holmes Blizzard Fan Oscillation Button Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Holmes Blizzard Fan</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">65026</has_guidid>
