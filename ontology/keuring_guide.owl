  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">How to Fix Keurig K75 Platinum Brewing System Water Pump Leak</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Keurig K75 Platinum Brewing System</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">13026</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/How+to+Fix+Keurig+K75+Platinum+Brewing+System+Water+Pump+Leak/13026</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_45272">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Before beginning the repair, turn off the brewer and disconnect it from any power source. Move the brewer so that the metal baseplate is easily accessible. Remove the five silver 4 mm Phillips #2 screws from the baseplate.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_45273">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a spudger, gently pry the metal baseplate from the brewer. Gently set down the baseplate.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_44861">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two silver 4mm Phillips #2 screws that hold the water pump in place.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_44860">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Realign the rubber gasket of the water pump to create a tight fit.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_13031">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <uses_tool rdf:resource="#Tool_ph00_phillips_screwdriver"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <has_step rdf:resource="#Step_44850"/>
  <has_step rdf:resource="#Step_44852"/>
  <has_step rdf:resource="#Step_44854"/>
  <has_step rdf:resource="#Step_44855"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">How to Keurig K75 Platinum Brewing System</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Keurig K75 Platinum Brewing System</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">13031</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/How+to+Keurig+K75+Platinum+Brewing+System/13031</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Tool_ph00_phillips_screwdriver">
  <rdf:type rdf:resource="#Tool"/>
