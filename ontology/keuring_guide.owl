<owl:NamedIndividual rdf:about="#Step_58793">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift and remove the old battery by pulling the tabs on both sides. Gently replace with a new battery.</step_text>
</owl:NamedIndividual>

