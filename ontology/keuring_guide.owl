  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nutri Ninja Ninja Blender Duo BL641 Motherboard Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nutri Ninja Ninja Blender Duo BL641</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">98324</has_guidid>
