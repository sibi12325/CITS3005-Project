  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This video shows how to remove the old parts from the Gaggia Classic espresso machine.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_136383">
  <rdf:type rdf:resource="#Step"/>
