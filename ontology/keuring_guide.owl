  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate and remove the two 9.5 mm Phillips #1 screws. Remove the plastic ring around the K-Cup Pack Holder</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_133177">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 9.5 mm Phillips #1 screws on the underside of the lid.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_133178">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Detach the lid from the Keurig.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_133179">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate and remove the two 9.5 mm Phillips #1 screws on the top of the puncture needle.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_133180">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Squeeze and pull on the hose to remove it from the Puncture Needle Puncture needle should now be exposed</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_133181">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the Puncture Needle Assembly from Keurig</step_text>
</owl:NamedIndividual>

