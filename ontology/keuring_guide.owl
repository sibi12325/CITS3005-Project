  <has_step rdf:resource="#Step_165255"/>
  <has_step rdf:resource="#Step_164194"/>
  <has_step rdf:resource="#Step_164200"/>
  <has_step rdf:resource="#Step_164205"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rydis H68 Pro Motherboard Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rydis H68 Pro</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">87042</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Rydis+H68+Pro+Motherboard+Replacement/87042</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_160794">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the vacuum over so that the wheels are facing you. Use a Phillips #1 screwdriver to remove the four 10mm screws that surround the battery cover.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_160795">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift up the battery cover to reveal the battery. Remove the battery.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_164269">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the eight 10 mm Phillips #1 screws.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_164276">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hold the front bumper and pull it off. It requires a lot of force to pull the bumper off.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_164284">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pry off the back bumper by going around the entire top of the bumper with the metal spudger.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165255">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">To access the motherboard entirely, lift the top portion of the vacuum off.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_164194">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the four main 10mm screws on the underside of the motherboard.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_164200">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a Phillip's #1 screwdriver to unscrew and remove the four 10 mm screws.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_164205">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pinch the two tabs together, shown here, to loosen the motherboard out of the vacuum. Lightly pull on the silver box shown to lift the motherboard out.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_87043">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
