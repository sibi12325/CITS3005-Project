  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">You can now pull the handle off of the griddler and reattach a working handle.</step_text>
</owl:NamedIndividual>

