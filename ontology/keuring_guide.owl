  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Waring+Pro+SB30+Indicator+Lights+Replacement/30859</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_72144">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using two thin screwdrivers (or similar objects) wedge them into the slots of the plastic shell of the desired light.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_72146">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Push the screwdrivers inward and guide the bulb outwards. Gently grab by the wires and remove bulb from plastic shell.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_30875">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_8_needle_nose_plier"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <uses_tool rdf:resource="#Tool_thin_knife"/>
