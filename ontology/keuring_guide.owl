<owl:NamedIndividual rdf:about="#Step_155230">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Press the button to disconnect the vacuum head from the connecting pole.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_155233">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Turn the end cap counterclockwise using the iFixit Opening Tool. If you do not have an iFixit Opening Tool, a quarter may be used instead.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_155235">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the brush bar out of the housing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_155254">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Turn the padlock key counterclockwise using the iFixit Opening Tool. If you do not have an iFixit Opening Tool, a quarter may be used instead.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_155249">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t8_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the three front 1.4 cm  torx screws using the T8 Torx screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156694">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the upper plate once the padlock is unlocked.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_155267">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t8_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the top two corner 1.4 cm torx screws using the iFixit T8 Torx screwdriver. Remove axie and roller after the screws are taken out.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_155274">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the drive assembly from vacuum housing. Red wire connecting the drive assembly to housing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_155284">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pop off the circular plating to remove the housing from the connection piece. Red wire connects through the upper portion of the housing.</step_text>
</owl:NamedIndividual>

