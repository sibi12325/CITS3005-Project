</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_79095">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The trigger button is held in with light tension clips. Gently pry them back to release.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_75582">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the white trigger.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_75585">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Desolder the contact points between the trigger button and the red wire and also where the plug attaches to the trigger button Be sure to reference the how to solder video posted to the device page</step_text>
</owl:NamedIndividual>

