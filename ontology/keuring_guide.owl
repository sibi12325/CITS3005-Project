</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_209001">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">15</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pry it out from the frame by using the metal spudger.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_209002">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">16</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the Mini Pump up and out of the frame using your hands.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_209003">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">17</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the wires connecting the Mini Pump to the Keurig, including the air tube. The Mini Pump is should now be free from the brewer.</step_text>
</owl:NamedIndividual>

