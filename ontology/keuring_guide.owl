<owl:NamedIndividual rdf:about="#Step_149634">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the screws holding the feeding sensor in place. The screw length is 10 mm and the head diameter is 6 mm. Lift and remove the sensor. Replace the faulty sensor with a new sensor.</step_text>
</owl:NamedIndividual>

