  <has_step rdf:resource="#Step_193731"/>
  <has_step rdf:resource="#Step_193722"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Insignia Ice Maker NS-IMP26SS7 Fan Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Insignia Ice Maker NS-IMP26SS7</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">103596</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Insignia+Ice+Maker+NS-IMP26SS7+Fan+Replacement/103596</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_193719">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Undo the 6 Screws found on the bottom edge holding the rear casing on.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_193720">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the casing off.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_193721">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the fan on the side of the Ice maker and undo the 4 screws holding it in place.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_193731">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the cover from the circuit board by pressing against the lever to the side. A pry tool may be needed.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_193722">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Follow the fan's power cable and unplug. You may have to cut a zip tie holding the cable in place.</step_text>
</owl:NamedIndividual>

