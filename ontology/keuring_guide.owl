</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_192527">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t15_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">16</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove two 16mm screws by rotating counter-clockwise with a T15 Torx screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_192973">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">17</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the belt-assembly unit by pulling the belt-assembly unit away from the motor.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_192975">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_cutting_plier"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">18</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rotate the motor unit over so that the wires are exposed. Cut wires with cutting pliers to release the motor.</step_text>
</owl:NamedIndividual>

