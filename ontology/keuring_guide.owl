</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59214">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gently lift the silver support bracket freeing the heating plate from the heating coil.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59215">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_needlenose_pliers"/>
