<owl:NamedIndividual rdf:about="#Step_162513">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the device upside down. Locate the two (2) 20mm screws on the batttery plate.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162514">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <uses_tool rdf:resource="#Tool_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the Phillips #1 screwdriver to loosen both screws.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162515">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the battery back plate to reveal the battery.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162516">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disconnect the current battery by pulling the connector from the base and pushing on the clip.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_164099">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the device upside down.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_164106">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the screwdriver to take off the four (4) 10mm screws located near the brush which secure the plastic guard.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_164108">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the plastic brush guard. Pull off the plastic covering the brush.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165079">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the brush to clean or replace.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162519">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip device upside down. Locate the six (6) screw connected to the top cover.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162520">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t10_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove eight (8) star screws using the T10 bit.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165086">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull off one side of the brush front cover, and then pull off the other.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165089">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">11</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip right side up and turn around. FInd the two clips connecting the top cover to the body of the device.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162526">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">12</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift up on the top cover gently. Disconnect the connector from  the motherboard to detach the top cover from the robot.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_86236">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <has_step rdf:resource="#Step_162531"/>
  <has_step rdf:resource="#Step_162532"/>
  <has_step rdf:resource="#Step_162534"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mr. Coffee DRX5 Digital Display Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mr. Coffee DRX5</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">86236</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Mr.+Coffee+DRX5+Digital+Display+Replacement/86236</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162531">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a Phillips #1 screwdriver to remove the two 9mm screws  and the three 11mm screws on the bottom of the coffee maker and remove the bottom plastic cover.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162532">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Slide display out of base housing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162534">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unplug display from back</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_86266">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <has_step rdf:resource="#Step_162660"/>
  <has_step rdf:resource="#Step_162661"/>
  <has_step rdf:resource="#Step_162663"/>
  <has_step rdf:resource="#Step_162676"/>
  <has_step rdf:resource="#Step_163113"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Black and Decker TR1278B Top Cover Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Black and Decker TR1278B</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">86266</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Black+and+Decker+TR1278B+Top+Cover+Replacement/86266</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162660">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unplug the toaster before disassembly. Remove the carriage lever by pulling it away from the toaster.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162661">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Toaster will be turned upside down. Clear crumb tray or work over a paper towel. Turn the toaster upside down. Unscrew all four 4mm screws on the bottom using a Phillip's head #2 (PH2) screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162663">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Detach the bottom from the front cover using the plastic opening tool.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162676">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Turn the toaster right side up. Support the bottom. The ribbon cable that connects the toaster to the front cover can be damaged. Lift the front cover. Lift carefully. Again the ribbon cable can be damaged.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163113">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Detach the bundled cable connector from the toaster by carefully pulling it away from the circuit board.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_86304">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <uses_tool rdf:resource="#Tool_flathead_screwdriver"/>
  <has_step rdf:resource="#Step_162786"/>
  <has_step rdf:resource="#Step_162787"/>
  <has_step rdf:resource="#Step_162788"/>
  <has_step rdf:resource="#Step_163481"/>
  <has_step rdf:resource="#Step_163486"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">iRobot Braava 320 Body Shell Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">iRobot Braava 320</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">86304</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/iRobot+Braava+320+Body+Shell+Replacement/86304</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162786">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip your device over and locate the battery cover. The battery cover is located in the center of the device in between both wheels.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162787">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_flathead_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a Flathead #4 screwdriver, gently turn the 12mm screw that holds the battery cover in. The screw does not remove, it stays attached to the cover.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162788">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gently remove the battery from the  tray and disconnect the battery  cable. To remove the battery, pull it straight up. To disconnect the cable, gently pull it straight out of the connector. To reinstall the cable, hold it near the end and gently push it in the connector.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163481">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">With the battery removed, rotate your device so the front of it is facing you. Remove the four 10mm Phillips #2 screws that attach the shell.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163486">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using both hands, hold the device together while you flip it over. Starting from the front, gently lift up the shell and slowly tilt back until it is  fully removed.</step_text>
</owl:NamedIndividual>

