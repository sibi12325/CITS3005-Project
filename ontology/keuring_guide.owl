</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_58829">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a Phillips #1 screwdriver to remove the two captive mounting screws on the side wheel motor assembly.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_58830">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gently pull on motor assembly to remove.</step_text>
</owl:NamedIndividual>

