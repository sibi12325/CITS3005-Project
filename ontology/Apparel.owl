  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TomTom Runner Battery  Replacement Guide</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TomTom Runner</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">87059</has_guidid>
