  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">How to remove scratches from a Breitling Navitimer sapphire watch face</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Breitling Navitimer</has_category>
