<owl:NamedIndividual rdf:about="#Step_108671">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Put on the pants inside out.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_108672">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_safety_pin"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Safety pin the waistband of the pants to the desired fit.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_108673">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_pin"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pin the outside seam around both hips so that the pants are snug.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_108674">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_pin"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pin down the back seam and inseam so that the pants fit comfortably.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_108675">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_sewing_machine"/>
  <uses_tool rdf:resource="#Tool_pin"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Take the pants off. Sew the back seam, following your pins. Be careful when operating a sewing machine.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_108676">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_sewing_machine"/>
  <uses_tool rdf:resource="#Tool_pin"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Put the pants on.  Pin the rest of the inseam and the outside seam down both legs. Our preference was to use the skinny jean cut. However, this sewing technique can be applied to any jean cut by simply adjusting the snugness of the fit as you pin.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_108677">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_fabric_scissors"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Take the pants off. Then, cut off the extra fabric. If you prefer, iron your newly created seams so that they lay open.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_108678">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Enjoy your newly fitted pair of pants!</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_50228">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <uses_tool rdf:resource="#Tool_ph000_phillips_screwdriver"/>
