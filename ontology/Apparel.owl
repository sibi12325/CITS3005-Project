<owl:NamedIndividual rdf:about="#Step_108431">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">With a plastic opening tool, disconnect the four connectors on the top of the motherboard.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_108432">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_ph00_phillips_screwdriver"/>
  <uses_tool rdf:resource="#Tool_ph000_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove three 3.0 mm Phillips screws on the edges of the motherboard with a  Phillips #000 screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_108433">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Grasp the edges of the motherboard and slide it slightly towards the right to remove it from its case.</step_text>
</owl:NamedIndividual>

