<owl:NamedIndividual rdf:about="#Step_188326">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The battery is secured to the back of the motherboard utilizing a sticky pad. Gently separate the battery from the board while making sure not to put too much stress on the board. This is best done at the location shown.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_188329">
  <rdf:type rdf:resource="#Step"/>
