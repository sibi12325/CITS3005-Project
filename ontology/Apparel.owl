  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">How do I replace a broken zip slider on my Vaude clothing?</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Zipper</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">69102</has_guidid>
