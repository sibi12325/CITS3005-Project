<owl:NamedIndividual rdf:about="#Step_165225">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Peel back the metal cover on the end of the watchband.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165795">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t3_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a T3 Torx screwdriver to unscrew the four 6 mm screws on the corners of the  metal cover plate.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165804">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the plastic opening tool to pry off the metal cover plate.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165806">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Detach the black ribbon cable by using the tweezers.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165810">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Detach the orange press-fit (or "pop") connector by using the tweezers.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165812">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pry out the battery using the plastic opening tool.</step_text>
</owl:NamedIndividual>

