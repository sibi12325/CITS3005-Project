  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Samsung Gear Fit</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">25518</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Samsung+Gear+Fit+Battery+Replacement/25518</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_64484">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">With the  motherboard removed, the battery is exposed. A small metal strut protects the battery. Remove the small metal strut by prying it out of both sides of the enclosure.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_64487">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use caution when handling the battery.  If damaged or punctured, it can release hazardous chemicals or catch fire. Remove the battery by pulling on the pull tab. Don't be afraid to use force, the battery is tightly packed into the plastic enclosure.</step_text>
</owl:NamedIndividual>

