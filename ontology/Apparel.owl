</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_126398">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_ph000_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the four highlighted screws from the motherboard using the Philips screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_126402">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the metal tweezers to lift the motherboard out of the device.</step_text>
</owl:NamedIndividual>

