</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_111265">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the tabs on the motherboard and unclasp them.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109352">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the spugder to gently pry upwards on the motherboard. If you keep the battery attached to the upper half of the motherboard, you will have to gently pry upwards on both the battery and the motherboard.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_111263">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Once the motherboard and the battery are removed, disconnect the battery at the connector.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_111838">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a spudger to  gently pry upward on the clasp connecting the battery to the motherboard.</step_text>
</owl:NamedIndividual>

