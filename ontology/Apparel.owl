  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the four 3.5mm screws using a T5 screwdriver to open the faceplate.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_161455">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t5_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 1.8mm silver screws using the PH00 screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_161456">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Slide the spudger under the gold-plated connection ribbon and lift to disconnect from the motherboard.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163962">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull up on the motherboard using tweezers to detach it from the watch.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_85189">
