</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163406">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_ph00_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a Philips head #00 screwdriver, remove the two 3.6 millimeter screws.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163417">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_heavy_duty_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a spudger, pry up the three cables.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163429">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_heavy_duty_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the flat end of a spudger, pry up the motherboard. Make sure the cable is fed through the hole to avoid damaging it. The motherboard is attached to the battery casing, so place the motherboard in the orientation shown once freed.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_86706">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_heat_gun"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
