  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">How to Darn a Hole in a Knitted Garment</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Patagonia Sweater</has_category>
