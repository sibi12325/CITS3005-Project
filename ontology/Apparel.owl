</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Tool_needle">
  <rdf:type rdf:resource="#Tool"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">needle</rdfs:label>
