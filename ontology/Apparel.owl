<owl:NamedIndividual rdf:about="#Tool_paper_towels">
  <rdf:type rdf:resource="#Tool"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">paper towels</rdfs:label>
