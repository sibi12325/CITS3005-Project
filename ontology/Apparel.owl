  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Garmin Forerunner 25</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">106340</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Garmin+Forerunner+25+Motherboard+Replacement/106340</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_201824">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t4_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the two T4 6 mm screws located on both sides of the motherboard.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_202863">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the motherboard up using a plastic spudger. Ribbon cables are still attached underneath—be careful when prying the motherboard up.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_202864">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the ZIF connectors using a plastic spudger tool. Remove the motherboard.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_106345">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <uses_tool rdf:resource="#Tool_t5_torx_screwdriver"/>
