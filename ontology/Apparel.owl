<owl:NamedIndividual rdf:about="#Tool_spring_bar_removal_tool">
  <rdf:type rdf:resource="#Tool"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">spring bar removal tool</rdfs:label>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.amazon.com/SWISS-REIMAGINED-TL-SPBR-001-Spring-Jeweler/dp/B00DMUOMEU/</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_108454">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spring_bar_removal_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The straps are fastened to the watch with a spring  bar, which can be collapsed when force is placed on the bar's edge. Pull the strap back to reveal the edge of the spring bar. With a Flathead Screwdriver or Spring Bar Removal Tool, press down on the edge of the spring bar and pull it back until it comes out of its socket.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_108456">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spring_bar_removal_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Once the spring bar is released, pull out the other end to detach the strap.</step_text>
</owl:NamedIndividual>

