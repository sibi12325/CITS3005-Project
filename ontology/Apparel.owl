  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">4 putty knife</rdfs:label>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">/Item/4%22_Putty_Knife</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Tool_utility_knife">
