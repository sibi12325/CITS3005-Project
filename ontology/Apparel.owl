</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Tool_wire_cutter">
  <rdf:type rdf:resource="#Tool"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">wire cutter</rdfs:label>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ifixit.com/Tools/Flush-Wire-Cutter/IF145-056</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_139006">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Select a replacement slider and zip stop.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_139007">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_wire_cutter"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the zip stop at the end of the zip by prying it open with a wire cutter and pulling it away.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_139008">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the broken slider.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_139009">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_wire_cutter"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Insert the new slider onto the zip and use the wire cutter to press the zip stop into the position where the old stop had been.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_139010">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Positon the zip stop together with the help of the tweezers.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_139011">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pinch the zip stop together with the help of the tweezers.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_139012">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the zip puller (black) from the slider and thread it onto the new slider.</step_text>
</owl:NamedIndividual>

