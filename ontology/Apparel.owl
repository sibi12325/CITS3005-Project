  <has_step rdf:resource="#Step_183204"/>
  <has_step rdf:resource="#Step_183210"/>
  <has_step rdf:resource="#Step_183453"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Motorola Moto 360 2nd Gen Vibration Motor Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Motorola Moto 360 2nd Gen</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">99232</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Motorola+Moto+360+2nd+Gen+Vibration+Motor+Replacement/99232</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_183202">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_iopener"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Set the watch face down on a flat surface. Place the heated iOpener on the face of the rear cover to loosen the adhesive underneath.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_183203">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t3_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Carefully remove the inside of the watch. Carefully unscrew the two screw using the using the T3 Torx Screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_183204">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove any connectors on the motherboard using the tweezers and spudger. Carefully remove the battery by disconnecting the connector.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_183210">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">After removing the battery the vibration motor will be visible.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_183453">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a soldering iron, carefully remove the vibration motor by applying the tool under the vibration motor.</step_text>
</owl:NamedIndividual>

