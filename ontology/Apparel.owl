  <has_step rdf:resource="#Step_122777"/>
  <has_step rdf:resource="#Step_122781"/>
  <has_step rdf:resource="#Step_123150"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ASUS ZenWatch Battery Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ASUS ZenWatch</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">56678</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/ASUS+ZenWatch+Battery+Replacement/56678</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Tool_heavy_duty_spudger">
  <rdf:type rdf:resource="#Tool"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">heavy-duty spudger</rdfs:label>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ifixit.com/Tools/Heavy-duty-Spudger/IF145-013</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_126286">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">On the strap there is a knob. Sliding this knob towards the opposite side of the strap releases the tension on the spring bar, allowing you to pull the strap out..</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_122777">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_magnetic_project_mat"/>
  <uses_tool rdf:resource="#Tool_t5_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the T5 Torx driver to remove the four screws on the case. Use the magnetic pad to keep all your parts easily contained! The case will pry easily off with just gentle hand pressure</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_122781">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_heavy_duty_spudger"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the tweezers, gently remove the fabric protecting the battery connection. Take care when removing the fabric. Too much force may damage the contact with the metal tweezers. Use the heavy duty spudger to gently push the contact towards the top of the case.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_123150">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gently pry the battery up with the plastic pry tools</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_59388">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_magnetic_project_mat"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <uses_tool rdf:resource="#Tool_t5_torx_screwdriver"/>
  <uses_tool rdf:resource="#Tool_heavy_duty_spudger"/>
