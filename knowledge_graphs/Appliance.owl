<owl:NamedIndividual rdf:about="#Step_133178">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Detach the lid from the Keurig.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_133179">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate and remove the two 9.5 mm Phillips #1 screws on the top of the puncture needle.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_133180">
