<owl:NamedIndividual rdf:about="#Step_146026">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disconnect the electric plug from the wall.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_146027">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Turn off or push down off water main valve from the wall.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_146039">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_8_nut_driver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the back panel with 6 screws from the back panel using a 8 mm nut driver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_146043">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_13_mm_open_end_wrench"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disconnect the water supplied line from the refrigerator using two  13 mm open-end wrenches. Use a little bucket to receive the water from the water lines.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_146046">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_13_mm_open_end_wrench"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disconnect the water line from the water inlet valve using and open-end wrench size 15mm</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_146049">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_8_nut_driver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disconnect the water line cover removing 2 screws  using a 7 mm nut driver. Pull up the gum seal compound.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_146056">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Push the left side of the filter cover and pull it straight out.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_146059">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_8_nut_driver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the 2 screws  on top of the housing using  7 mm nut driver. Pull the housing and filter out slowly. Remove the filter by turn it  counterclockwise</step_text>
</owl:NamedIndividual>

