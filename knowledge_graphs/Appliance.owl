  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Krups 963</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">31392</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Krups+963+Power+Cord+Replacement/31392</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_74290">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Make sure the device is unplugged before continuing. Use a Phillips size 2 screwdriver to remove the four 12mm screws found on the bottom of the machine.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_74300">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the single black Phillips size 0 10mm length screw on the back of the machine.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_74304">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the bottom cover from the machine and slide it down the power cord.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_73394">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t5_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the PH2 (or similar) screw driver to remove the two 12mm screws holding the clear restraining brace. Remove the restraining brace from the machine base.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_73395">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t5_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a 2.0 flat head (or similar) screw driver to loosen the screw holding the power cord in.</step_text>
</owl:NamedIndividual>

