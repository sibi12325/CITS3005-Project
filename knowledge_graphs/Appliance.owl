  <has_step rdf:resource="#Step_94320"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mr. Coffee BVMC-SJX33GT Valve Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mr. Coffee BVMC-SJX33GT</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">40933</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Mr.+Coffee+BVMC-SJX33GT+Valve+Replacement/40933</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_94319">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">After pulling the second tube out, locate the bulging part of the hose that indicates the valve's placement.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_94320">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Grip the tube and push the valve out the end of the hose. Remember the direction of the valve when you pull it out because this valve is one way. If you insert it or reinsert it the wrong way, water will not flow through the coffee maker.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_40935">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <uses_tool rdf:resource="#Tool_needlenose_pliers"/>
