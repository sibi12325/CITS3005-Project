<owl:NamedIndividual rdf:about="#Step_190152">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Attach a new screen to the screen protector. Make sure that the charging port remains accessible, as it can easily become detached during the replacement.</step_text>
</owl:NamedIndividual>

