  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_8_needle_nose_plier"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the needle nose pliers to turn the coupling clockwise to loosen it.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_72000">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rotate the blender 90 degrees clockwise. Remove the two #8 Phillips 12 mm screws using the Phillips #2 screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_72167">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_thin_knife"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Slide the knife in the metal loop holding the insulation before the white copper wire, and wiggle the knife back and forth until it loosens. Pull the wire out from the metal loop that you loosened.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_72002">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the motor up out of the chassis.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_30760">
