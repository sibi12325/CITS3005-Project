<owl:NamedIndividual rdf:about="#Step_39616">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unplug the washing machine! Carefully lay washing machine on its back.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_39619">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_socket_wrench"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Carefully remove both wire harnesses connected to the pump. Squeeze and pull back hose clamp. Twist drain hose off of pump. Using a 1/4" socket wrench, remove the three mounting screws. Pull the pump out of place. Pulling the pump away takes a little force.</step_text>
</owl:NamedIndividual>

