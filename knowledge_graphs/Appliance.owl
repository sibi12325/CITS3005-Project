<owl:NamedIndividual rdf:about="#Step_114683">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove to electrical cap to the positive terminal(black terminal), and undo the wiring.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_114684">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the electrical cap to the negative end(red wire), and undo the wiring. Once this is complete reconstruct the wiring with the new motor.</step_text>
</owl:NamedIndividual>

