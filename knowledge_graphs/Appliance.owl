</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_130671">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the four 18mm PH2 screws holding the motor to the main shredder assembly.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_130672">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_solder"/>
  <uses_tool rdf:resource="#Tool_soldering_station"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disconnect the motor from the motherboard by either clipping the wires, or using a soldering iron to disconnect the wires directly at the board. To reattach the motor, solder the wires back together, or to the motherboard depending on how you removed them.</step_text>
</owl:NamedIndividual>

