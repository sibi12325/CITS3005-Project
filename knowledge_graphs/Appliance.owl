</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163487">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Slide display out of the way.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163489">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a Phillips #1 screwdriver, remove 12mm screws from heating element bracket and remove bracket.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163490">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull heating element out of the way.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163493">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Twist warming plate clockwise so that the tab is out of place. Remove warming plate.</step_text>
</owl:NamedIndividual>

