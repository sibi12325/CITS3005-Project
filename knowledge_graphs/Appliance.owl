<owl:NamedIndividual rdf:about="#Step_118835">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Carefully pop off the cover using a spudger or other opening tool.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_118836">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_jis_1_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a JIS #1 screw driver to remove both 25mm bolts that are attaching the door to the refrigerator.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_118837">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the damaged hinge. Make sure that you don't lose the bolt that goes into the door.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_118821">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the door by slowly lifting straight up, put the door off to the side.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_118822">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Get a new, non-damaged door.</step_text>
</owl:NamedIndividual>

