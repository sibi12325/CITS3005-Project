  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Once you have the motor and switch by themselves it should look like this.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_168108">
  <rdf:type rdf:resource="#Step"/>
