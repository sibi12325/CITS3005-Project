<owl:NamedIndividual rdf:about="#Guide_47849">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_safety_glasses"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
