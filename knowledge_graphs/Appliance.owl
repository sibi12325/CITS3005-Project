  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">74111</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Motherboard+Replacement/74111</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_149580">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_soldering_station"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Desolder the wires from the motherboard. Learn more about soldering components[https://www.ifixit.com/Guide/How+To+Solder+and+Desolder+Connections/750| here!]</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_149583">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">After all of the wires are removed, the motherboard can be lifted off and removed with ease.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_74116">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_ph000_phillips_screwdriver"/>
  <uses_tool rdf:resource="#Tool_wire_stripper_crimping_tool"/>
  <has_step rdf:resource="#Step_149561"/>
  <has_step rdf:resource="#Step_149566"/>
  <has_step rdf:resource="#Step_149571"/>
  <has_step rdf:resource="#Step_149573"/>
  <has_step rdf:resource="#Step_149616"/>
  <has_step rdf:resource="#Step_149619"/>
  <has_step rdf:resource="#Step_149625"/>
  <has_step rdf:resource="#Step_149630"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fellowes T7CM  Main Power Cord Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fellowes T7CM</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">74116</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Fellowes+T7CM++Main+Power+Cord+Replacement/74116</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_149616">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_wire_stripper_crimping_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the wire holder. This is a small screwed in piece of plastic which holds the wires in place. The screw length is 10 mm and the head diameter is 6 mm.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_149619">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the blade housing from the external case. Remove the three screws from the blade housing. The screw length is 18 mm and the head diameter is 5 mm</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_149625">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_wire_stripper_crimping_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use Caution when lifting the blade housing from the casing. '''Warning: '''Sharp Objects are now exposed. Place the blade housing close to the case to ensure no damage occurs to the connected wires and that they remained attached to the casing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_149630">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_wire_stripper_crimping_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the main power cord. Clip the wire and strip the remaining end of the wire. Twist the new cord together with the remaining  old wire. Use electrical tape in order to cover the wire splice.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_74119">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_ph000_phillips_screwdriver"/>
  <uses_tool rdf:resource="#Tool_wire_stripper_crimping_tool"/>
