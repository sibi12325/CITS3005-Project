<owl:NamedIndividual rdf:about="#Step_58837">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_flathead_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a flat-head screwdriver to separate the metal flap from the metal plate.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_58846">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_needlenose_pliers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Grasp the metal flap with the large needle nose pliers. Unbend metal flap until at a 90 degree angle from the metal plate. Repeat these steps for the second metal flap.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_58851">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_needlenose_pliers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the large need nose pliers to grab the  metal panel. Pull the metal panel away from the body of the toaster to dislodge the flaps.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_58853">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Push the metal flaps towards the body of the toaster to access the bread lift shelf.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_58856">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Push down on the opposite side of the bread lift shelf to expose the metal flaps.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_58860">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull up on the bread lift shelf to remove from the toaster.</step_text>
</owl:NamedIndividual>

