  <has_step rdf:resource="#Step_85688"/>
  <has_step rdf:resource="#Step_85695"/>
  <has_step rdf:resource="#Step_85713"/>
  <has_step rdf:resource="#Step_85717"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Motorola Moto 360 O-Ring Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Motorola Moto 360</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">37849</has_guidid>
