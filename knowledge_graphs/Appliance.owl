  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using pliers or tweezers, remove the connectors of the four wires attached to the circuit board.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_192690">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Grab the wires and tilt the edge of the board up to lift the circuit board up and out.</step_text>
</owl:NamedIndividual>

