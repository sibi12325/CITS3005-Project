<owl:NamedIndividual rdf:about="#Step_125270">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Separate the power unit assembly (the black top) from the tank assembly (the red bottom). To open, grab the black clips on the side and pull out to un-hook from the red bottom.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_125273">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove foam sleeve from filter cage</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_125280">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_allen_wrench"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the 2 H3.5 Allen screws attached to the filter cage.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_125282">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Take out the filer cage, leave the black float, and replace the filter cage.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_56491">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_t10_torx_screwdriver"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
