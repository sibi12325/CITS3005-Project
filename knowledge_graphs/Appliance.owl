  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Samsung Gear S2 Back Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Samsung Gear S2</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">86697</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Samsung+Gear+S2+Back+Replacement/86697</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163388">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Insert a plastic opening tool between the top and bottom portions of the case, and gently pry off the back. Do not insert the tool more than 3 millimeters into the case. Inserting more than 3 mm may damage the seal and electric components.</step_text>
</owl:NamedIndividual>

