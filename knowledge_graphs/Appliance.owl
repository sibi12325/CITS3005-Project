  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Make sure the device is unplugged before continuing. Turn the Kitchen Gourmet upside down.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_85908">
  <rdf:type rdf:resource="#Step"/>
