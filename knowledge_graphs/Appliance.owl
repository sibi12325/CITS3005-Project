  <has_step rdf:resource="#Step_149619"/>
  <has_step rdf:resource="#Step_149625"/>
  <has_step rdf:resource="#Step_149630"/>
  <has_step rdf:resource="#Step_149634"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fellowes T7CM Trigger Sensor Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fellowes T7CM</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">74119</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Fellowes+T7CM+Trigger+Sensor+Replacement/74119</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_149634">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the screws holding the feeding sensor in place. The screw length is 10 mm and the head diameter is 6 mm. Lift and remove the sensor. Replace the faulty sensor with a new sensor.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_75360">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_ph0_phillips_screwdriver"/>
  <uses_tool rdf:resource="#Tool_spanner_3.0_screwdriver"/>
  <has_step rdf:resource="#Step_152474"/>
  <has_step rdf:resource="#Step_152479"/>
  <has_step rdf:resource="#Step_152480"/>
  <has_step rdf:resource="#Step_152493"/>
