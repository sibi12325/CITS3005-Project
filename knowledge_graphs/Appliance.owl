  <has_step rdf:resource="#Step_75650"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Braun Aromaster KF440 one-way valve Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Braun Aromaster KF440</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">32361</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Braun+Aromaster+KF440+one-way+valve+Replacement/32361</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_75645">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the carafe (coffee pot) from the coffee maker. Turn the Braun Aromaster KF 440 upside down so the base is exposed.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_75648">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_needlenose_pliers"/>
  <uses_tool rdf:resource="#Tool_basic_pliers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the nut at the center of the base and unscrew it with a pair of basic pliers.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_75649">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gently lift the cover away from the bottom of the coffee maker and remove it.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_75643">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the one-way valve.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_75640">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift up on the one way valve to snap it out of place. (This shouldn't require much force).</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_75641">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the larger plastic opening tool, gently push the tubing off of the one-way valve. (If the tubing is being stubborn and won't come off, run the metal spudger around the inside of tubing to free it up.)</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_75650">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Completely remove one-way valve from the inside of the coffee maker.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_32367">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_basic_pliers"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
