  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Garmin Fenix 2</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">60945</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Garmin+Fenix+2+Motherboard+Replacement/60945</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_127310">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a spudger to pry open the power ribbon from the main board.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_127311">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t9_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use your Torx T9 screwdriver to remove the one  .8mm screw that holds the main board to the watch housing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_127317">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the spudger to pry the motherboard out of the housing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_127312">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Once removed simply separate the digitizer from the main board.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_127313">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lastly use your spudger tool to pry the data ribbon open from the motherboard. Once the ribbon is removed the board will be free and you can replace the board.</step_text>
</owl:NamedIndividual>

