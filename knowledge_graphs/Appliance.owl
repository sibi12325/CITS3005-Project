  <uses_tool rdf:resource="#Tool_tweezers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use your tweezers to remove the metal ring holding the hose to the plastic end piece and replace the hose / bit as needed.</step_text>
</owl:NamedIndividual>

