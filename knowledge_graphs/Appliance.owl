<owl:NamedIndividual rdf:about="#Step_126225">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_heavy_duty_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">With a flat metal spudger pry back the little metal clamps that hold the motor secure. Do not bend these too far as they can easily break off!</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_126233">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <uses_tool rdf:resource="#Tool_soldering_station"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">With the soldering iron, heat the two solder tabs on back of motor. Use metal tweezers to pull up and remove motor. A soldering iron gets hot! Take care not to burn yourself or place it on any other part of the board.</step_text>
</owl:NamedIndividual>

