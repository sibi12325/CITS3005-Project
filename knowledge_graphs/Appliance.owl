</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163450">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using tweezers, lift the battery casing. Make sure the cables are fed through their gaps to avoid damaging them.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163460">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_heavy_duty_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the battery casing over and using a spudger, push on the battery to free it from the casing. Once the battery is free, use the flat end of a spudger to lift up the battery cable.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_86719">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_heavy_duty_spudger"/>
  <uses_tool rdf:resource="#Tool_t4_torx_screwdriver"/>
  <uses_tool rdf:resource="#Tool_t5_torx_screwdriver"/>
