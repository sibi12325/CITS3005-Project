<owl:NamedIndividual rdf:about="#Step_108427">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t4_torx_screwdriver"/>
  <uses_tool rdf:resource="#Tool_t5_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Power off your watch before disassembling it. Place the watch upside down on a hard surface so that its back case is facing upright. Use a T4 Torx Star screwdriver to remove the four 4.0 mm Torx screws from the corners of the back case.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_108429">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a plastic opening tool to pry open the back case from both sides of the watch.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_108430">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the back case.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_50124">
