<owl:NamedIndividual rdf:about="#Step_58855">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Press down on the toaster handle to access to the grabber spring located behind the large metal plate.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_58852">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the grabber spring.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_110083">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_needlenose_pliers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the large needle nose pliers to unwind the grabber spring from one side of the carriage to remove. Repeat this step for the opposite side.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_60109">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the old spring from the toaster unit to insert new.</step_text>
