<owl:NamedIndividual rdf:about="#Step_39078">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_needlenose_pliers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">15</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the pliers and a wrench to straighten the outdoor mechanism.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_39080">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">16</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Take off the left side of the door by separating the unit from the hinge.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_39082">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">17</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Separate the right side of the door from the unit as in the previous step.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_39085">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">18</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Separate the door mechanism from the door by tilting the door to relieve the mechanism.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_39087">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">19</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the two door-handle screws.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_10973">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
