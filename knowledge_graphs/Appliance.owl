  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Samsung Gear Live Battery Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Samsung Gear Live</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">32301</has_guidid>
