  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Kenmore 110 Series Washing Machine</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">11041</has_guidid>
