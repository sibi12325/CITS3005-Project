  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hamilton Beach 70730 Drive Shaft Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hamilton Beach 70730</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">61834</has_guidid>
