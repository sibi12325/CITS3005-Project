</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_79091">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Desolder remaining contact point from the plug cord</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_77468">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a Phillips screwdriver, remove the two screws  holding the motor in place.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_77469">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the motor from the shell.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_33007">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_ph0_phillips_screwdriver"/>
