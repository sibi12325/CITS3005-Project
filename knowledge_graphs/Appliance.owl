<owl:NamedIndividual rdf:about="#Step_18605">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Slide out back bin by pressing the unmarked button on top of the Roomba.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_18608">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Slide out yellow bar on particle bin to access filter</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_18609">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This is the air filter assembly</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_18610">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pop out filter mount by prying 2 points that hold it to the rest of the assembly</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_18612">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Thoroughly clean or replace the air filter.  This can be done with compressed air or by rinsing it with water.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_18613">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reassemble the air filter assembly and slide the particle bin back into place</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_4343">
