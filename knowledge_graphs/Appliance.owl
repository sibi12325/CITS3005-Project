  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the cooling pipe from the top inside of the blender base and replace it.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_62059">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
