  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nutri Ninja BL450 Cooling Vent Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nutri Ninja BL450</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">62059</has_guidid>
