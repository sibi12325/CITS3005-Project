</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Tool_phillips_3_screwdriver">
  <rdf:type rdf:resource="#Tool"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">phillips 3 screwdriver</rdfs:label>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.amazon.com/TEKTON-26683-Phillips-4-Inch-Screwdriver/dp/B014US22SA/ref=sr_1_1?ie=UTF8&amp;qid=1493940423&amp;sr=8-1&amp;keywords=phillips+%233</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180020">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Put the cleaner into the upright position, turn the power off, and unplug the power cord.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180021">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the dirt cup from the cleaner. The HEPA filter is located under the dirt cup and under where it says HEPA. Twist the top counterclockwise to unlock the cap.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180022">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the HEPA filter out. Tap the filter against the inside of a trashcan to knock out the dirt and debris or replace as necessary. Do not wash the HEPA Filter.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180353">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_3_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the 4 retaining screws and remove the filter tray. Screw lengths are approximately  3/4".</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180356">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the motor housing vertically to dislodge the motor.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180357">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a '''plastic''' spudger or opening tool to press on the small circle and wiggle clips free.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_98259">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_ph0_phillips_screwdriver"/>
  <uses_tool rdf:resource="#Tool_star_bit_screwdriver"/>
  <has_step rdf:resource="#Step_180197"/>
  <has_step rdf:resource="#Step_180198"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nutri Ninja Ninja Blender Duo BL641 Suction Cups Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nutri Ninja Ninja Blender Duo BL641</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">98259</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Nutri+Ninja+Ninja+Blender+Duo+BL641+Suction+Cups+Replacement/98259</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Tool_star_bit_screwdriver">
  <rdf:type rdf:resource="#Tool"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">star bit screwdriver</rdfs:label>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.harborfreight.com/6-piece-star-bit-screwdriver-set-94757.html</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180197">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_star_bit_screwdriver"/>
  <uses_tool rdf:resource="#Tool_ph0_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Turn the blender upside down. Remove all screws that connect the bottom panel to the base of the blender (0.5 inch 5 gauge phillip's head). Note: 2 of the screws will be star shaped (in blue). Lift the bottom off of the base.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180198">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the damaged suction cup off of its pit.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_98261">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <has_step rdf:resource="#Step_180203"/>
  <has_step rdf:resource="#Step_180204"/>
  <has_step rdf:resource="#Step_180205"/>
  <has_step rdf:resource="#Step_180206"/>
  <has_step rdf:resource="#Step_180207"/>
  <has_step rdf:resource="#Step_180208"/>
  <has_step rdf:resource="#Step_180209"/>
  <has_step rdf:resource="#Step_180210"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Shark NV352 Roller Brush Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Shark NV352</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">98261</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Shark+NV352+Roller+Brush+Replacement/98261</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180203">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the latches on both sides of the vacuum to release the dust canister. Remove the dust canister from the vacuum by lifting it up.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180204">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lay down the vacuum to access the bottom panel of the roller brush compartment.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180205">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the metal spudger, pry the two small wheels located at the two bottom corners of the vacuum to remove them.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180206">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the eight 19mm Phillips #2 screws from the plastic cover. Remove the two 14 mm Phillips #2 screws from the upper section of the plastic cover.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180207">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">You will need to apply a strong force while pulling the wheels off because they are attached very tightly. Remove the wheels by pulling them out of their axles, away from the vacuum.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180208">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Perform the next step slowly and carefully so the wiring connecting the motherboard to the rest of the vacuum is not broken. Gently pull the grey lower part of the roller brush compartment away from the purple upper part of the compartment.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180209">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift up and then pull the roller belt off of the roller brush and the motor to remove it.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180210">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Take out the roller brush by lifting it out of its tray.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_98289">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <uses_tool rdf:resource="#Tool_soldering_station"/>
  <has_step rdf:resource="#Step_180203"/>
  <has_step rdf:resource="#Step_180204"/>
  <has_step rdf:resource="#Step_180205"/>
  <has_step rdf:resource="#Step_180206"/>
  <has_step rdf:resource="#Step_180207"/>
  <has_step rdf:resource="#Step_180208"/>
  <has_step rdf:resource="#Step_180209"/>
  <has_step rdf:resource="#Step_180210"/>
  <has_step rdf:resource="#Step_180250"/>
  <has_step rdf:resource="#Step_180666"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Shark NV352 Roller Brush Motor Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Shark NV352</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">98289</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Shark+NV352+Roller+Brush+Motor+Replacement/98289</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180250">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The wires are attached to the motor, so take out the motor slowly to ensure that they do not break. Remove the roller brush motor by carefully lifting it out of the vacuum.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180666">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Desolder the wires running to the motor from the motherboard. For details on how to desolder, visit this [https://www.ifixit.com/Guide/How+To+Solder+and+Desolder+Connections/750|soldering guide].</step_text>
</owl:NamedIndividual>

