  <has_step rdf:resource="#Step_167906"/>
  <has_step rdf:resource="#Step_167907"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">iRobot Roomba 870 Side Brush Motor Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">iRobot Roomba 870</has_category>
