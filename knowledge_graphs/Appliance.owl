  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Swiffer SweeperVac</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">31108</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Swiffer+SweeperVac+Battery+Replacement/31108</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_72775">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew seven 17.95 mm screws on the side of the device. Unscrew the 13.87 mm screw.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_72778">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Find the sticker on the back of the device. It may have already been removed. If there is a sticker, peel it back from either half of the device.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_72781">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Firmly, but carefully grip each half of the case and split it open.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_72782">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the silver-colored battery cap from the top of the battery.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_72783">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the attached battery from the device and place it on the surface. Remove the second silver-colored battery cap from the bottom of the interior of the device.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_72784">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the spudger, apply pressure to the lock on the black wire attachment. While pressure is being applied to the lock, pull the black wire away from the battery.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_72785">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the spudger, apply pressure to the lock on the red wire attachment. While pressure is being applied to the lock, pull the red wire away from the battery. When installing the new battery, you do not need to press down on the locks. Simply plug the wires into the appropriate sockets on the new battery.</step_text>
</owl:NamedIndividual>

