<owl:NamedIndividual rdf:about="#Step_169415">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the knob at the top of the fan.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_169418">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the knob by gripping it and pulling straight up. Use a spudger if it is difficult to grip. When reassembling, be sure to align the bottom of the new knob and the turning mechanism in the fan before pushing down.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_168457">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the plastic opening tool, turn the device over and remove the black plastic covers over the four screw holes.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_168716">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the four screws underneath the plastic covers.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_168715">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove 5 smaller screws visible through small holes between the base and the casing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_168717">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove four screws on the back, two near the top and two near the bottom.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_168718">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull off the back casing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_168719">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_size_1_and_2_ph_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the 2 screws that attach the front part of the casing to the motor.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_168720">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two middle screws attaching the black plastic cover to the front .</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_168722">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two screws attached to the white bar near the red light on the top of the front casing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_168723">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two screws keeping the  button attached to the top of the casing.</step_text>
</owl:NamedIndividual>

