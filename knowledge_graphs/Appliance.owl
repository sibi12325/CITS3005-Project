  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">How to Hem Pants by Hand-Sewing</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bottoms</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">50327</has_guidid>
