  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use your hands or a pair of needle nose pliers to remove the wire attached to the switch.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_74099">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_8_needle_nose_plier"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Grip either in the inside or outside of the switch with the needle nose pliers or your hand, and pull to remove the switch.</step_text>
</owl:NamedIndividual>

