</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163123">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the front cover.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163125">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the circuit behind the browning dial on the front cover. Do not lose the washers on the screws.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163127">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the circuit. The dial and buttons on the front cover will be loose.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163750">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Turn the knob on the circuit board left or right to adjust the browning dial.</step_text>
</owl:NamedIndividual>

