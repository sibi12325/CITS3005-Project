  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the Phillips #2 screwdriver, remove the four screws (10mm long, 5mm wide) on the circuit board.</step_text>
