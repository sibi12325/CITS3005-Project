</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_75483">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the vibrator motor on the front half of the disassembled watch. It is located in the corner, at the end opposite the green circuit board.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_75485">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the plastic opening tool, gently pry up the motor.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_75492">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the plastic prying tool, gently pry away the motor’s contacts and ribbon cable from the watch assembly</step_text>
</owl:NamedIndividual>

