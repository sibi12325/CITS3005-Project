</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_117320">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the blue plastic removal tool once again to carefully remove the screen.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_117322">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the plastic piece that is highlighted in the picture out from the back of the screen.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_117324">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
