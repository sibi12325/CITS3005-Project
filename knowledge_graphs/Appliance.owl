<owl:NamedIndividual rdf:about="#Step_39633">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">To avoid risk of electric shock, unplug the washing machine. Lay the washing machine on its back.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_39634">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_socket_wrench"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a 5/16" socket wrench, remove both screws from the belt cover. Remove the belt cover from the washer.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_39635">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_5_16_socket"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a 1/2" socket, remove the bolt securing the drive pulley. Remove the drive pulley and belt. When installing a new belt, wrap the belt around both the motor and drive pulleys before reinstalling the drive pulley.</step_text>
</owl:NamedIndividual>

