</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41167">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_ph0_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use Phillips head screwdriver and pliers to remove screw and nut securing transformer.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41168">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_2.5mm_flathead_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">If the wire gets pulled out of the transformer, reinsert it. Use a small flathead screwdriver to secure the screw that holds the wire in place.</step_text>
</owl:NamedIndividual>

