  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Keurig K40</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">56854</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Keurig+K40+Handle+Replacement/56854</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_123262">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t15_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew one of the 2mm screws on the handle using a T15 screw head. Keep a firm grip on the device so it does not shift out of place.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_123263">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the Keurig over and unscrew the other 2mm screw using the same screw head. Keep a firm grip on the device to prevent it from shifting or falling.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_123264">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the lever by pulling it away from the Keurig. Replace a new handle by putting the Keurig back together the same way it was taken apart.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_56905">
