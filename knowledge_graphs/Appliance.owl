  <has_step rdf:resource="#Step_181384"/>
  <has_step rdf:resource="#Step_181385"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Shark Rotator Lift-Away Speed Brushroll Motor Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Shark Rotator Lift-Away Speed</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">98638</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Shark+Rotator+Lift-Away+Speed+Brushroll+Motor+Replacement/98638</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181380">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t5_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the five 5mm Torx #9 screws on the front side.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181381">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t5_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the six 7.5mm Torx #20 screws on the bottom face of the nozzle. A screwdriver extension may be necessary to reach the screw closest to the center of the nozzle.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181383">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the top panel by lifting the top piece off of the bottom. There will be loud clicking noises.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181384">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t5_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">If you are trying to remove the belt, you may skip to the next step. Remove the 7.5mm Torx #20 screw in front of the motor. The motor is the gray block on the right side of the floor nozzle (when the nozzle faces towards you).</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181385">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t5_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">If you are trying to remove the belt, you may skip to the next step. Remove the panel to reveal the motor screw. Remove the 7.5mm Torx #20 motor screw. Remove the motor.</step_text>
</owl:NamedIndividual>

