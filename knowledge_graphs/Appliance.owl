</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_203049">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">11</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the four 12.0 mm Phillips head #2 screws on top of the electric valve.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_203050">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">12</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove both plastic pieces holding the electric valve in place.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_203052">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">13</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift electric valve out of place and tilt to the side to expose and remove the two 12.0 mm Phillips head #2 screws on the side.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_203057">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">14</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull apart to remove the electronic valve.</step_text>
</owl:NamedIndividual>

