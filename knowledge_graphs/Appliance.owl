</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162143">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Carefully place the spudger under the black cylindrical ring and pry it off the top.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162144">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the spring Remove the spring guide as well to obtain clear access to the screws</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162145">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove all four 4mm screws with a J1 Screwdriver Remove the clear cylindrical cap</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162146">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Apply pressure to both of the springs on the surface near the corkscrew, releasing the button port.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162148">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the malfunctioning buttons from the plastic frame.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_85493">
