  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the Roomba upside down. Using the Phillips #0 screwdriver, carefully remove the 5 mm screw holding the brush in place.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185482">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gently pull the brush up and remove it from the device.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185470">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the four 10mm screws on the underside of the Roomba using a Phillips #1 screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185474">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the bottom frame of the device.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185476">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Grab the green tape attached to the end of the battery and remove the battery from the device.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_99930">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_ph0_phillips_screwdriver"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
