  <has_step rdf:resource="#Step_186601"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">iRobot Roomba 595 Pet Series Wheel Spring Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">iRobot Roomba 595 Pet Series</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">100167</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/iRobot+Roomba+595+Pet+Series+Wheel+Spring+Replacement/100167</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_186592">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the four 10mm screws on the underside of the Roomba with a Phillips #1 screwdriver. Remove the Roomba's external underside.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_186597">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the 15 mm screws with the Phillips #0 screwdriver bit.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_186599">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Once the screws have been taken care of, the wheel component (with the spring inside) can be removed from the main assembly.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_186601">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 5mm screws in the component and gently twist the casing until the spring is exposed. Carefully remove the spring from the component assembly as shown</step_text>
</owl:NamedIndividual>

