  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Holmes Blizzard Fan Oscillation Gear Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Holmes Blizzard Fan</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">65029</has_guidid>
