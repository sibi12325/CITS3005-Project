<owl:NamedIndividual rdf:about="#Step_60049">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a Phillips #1 screwdriver to remove the 9.5mm screws from the side of the brush case, and pull to detach. Repeat for the opposite side of the brush case.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_60053">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a Phillips #1 screwdriver to remove the three 5.0mm screws from the back of the brush case. Pull off the back of the brush case.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_60054">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">11</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a Phillips #1 screwdriver to remove the two 1.5mm screws securing the chip in place. Remove the chip from its housing in the blue brush case.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_60055">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">12</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a Phillips #1 screwdriver to remove the two 5.5mm screws securing the motor in place.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_60056">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">13</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a Phillips #1 screwdriver to remove the two 4.5mm screws on the top and bottom of the metal plate. Pull out the motor and metal covering.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_60057">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">14</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a Phillips #1 screwdriver to remove the two 3.5mm screws on the corners of the circuit board. Gently pull out the circuit board.</step_text>
</owl:NamedIndividual>

