  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">With the Flathead 2.5mm screwdriver remove the 7.9mm screw from the side-brush.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_157708">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
