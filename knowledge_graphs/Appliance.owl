<owl:NamedIndividual rdf:about="#Step_186609">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove six screws (12mm long, 6mm head diameter) from bottom of the blender using a Phillips #2 screwdriver</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_189664">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove six screws (12mm long, 6mm head diameter) from component housing using Phillips #2 screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_189830">
  <rdf:type rdf:resource="#Step"/>
