<owl:NamedIndividual rdf:about="#Step_209005">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">15</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull out the white tab that has the wires that connect the Water Sensor to the water pump.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_209006">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">16</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pry the water sensor out of the frame on the base of the brewer and remove it from the device.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_108508">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <has_step rdf:resource="#Step_208901"/>
  <has_step rdf:resource="#Step_208903"/>
  <has_step rdf:resource="#Step_208904"/>
  <has_step rdf:resource="#Step_208905"/>
  <has_step rdf:resource="#Step_208906"/>
  <has_step rdf:resource="#Step_209521"/>
  <has_step rdf:resource="#Step_209522"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Wolf Gourmet WGBL100S Control Board Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Wolf Gourmet WGBL100S</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">108508</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Wolf+Gourmet+WGBL100S+Control+Board+Replacement/108508</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_208901">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the base of the blender upside-down. Remove the four 1/2" long screws in the corners of the base using a Phillips #2 screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_208903">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the metal spudger,  pry off one half of each rubber foot. Note that one side of the foot is glued down; pry off the other side.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_208904">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the same screwdriver, remove the next four 1/2" long screws in the holes under the feet. Pull the base off of the blender.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_208905">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull off the plastic housing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_208906">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the same Phillips #2 screwdriver, remove the two 3/4" long screws holding the control board in place.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_209521">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using pliers, pull off all the connectors on the top.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_209522">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull or pry off the last connector block, then remove the control board.</step_text>
</owl:NamedIndividual>

