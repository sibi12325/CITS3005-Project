  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ninja NJ600 LED Light Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ninja NJ600</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">41615</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Ninja+NJ600+LED+Light+Replacement/41615</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_95771">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the LED you wish to desolder. On the bottom of the board, each LED is designated with a small triangle with a line at one tip.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_96330">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_soldering_station"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Desolder the two LED leads, and remove the LED from the board. '''NOTE:''' the LED is a diode, and requires proper orientation when installed. Soldering iron is very hot, use caution when handling</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_41616">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <has_step rdf:resource="#Step_95783"/>
  <has_step rdf:resource="#Step_95794"/>
  <has_step rdf:resource="#Step_95804"/>
  <has_step rdf:resource="#Step_95809"/>
  <has_step rdf:resource="#Step_95810"/>
  <has_step rdf:resource="#Step_96793"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Capresso Perfectea 260 Motherboard Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Capresso Perfectea 260</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">41616</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Capresso+Perfectea+260+Motherboard+Replacement/41616</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_95783">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">First, take the base of the device and flip it over.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_95794">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the four 12 mm Phillips #2 screws surrounding the base then lift the bottom half of the base to expose the circuit boards inside the device.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_95804">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 7.5 mm Phillips #2 screws holding in the motherboard</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_95809">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unplug the wire going into the side of the motherboard</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_95810">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the metal spudger to cut open the rubber covering the latch where it clamps onto the copper tab. Unplug wire at heating element by first using the metal spudger to push the latch in and then pushing it out. Be careful when unplugging the wire as if it is done too forcefully it can damage the connection and make it difficult to connect a new motherboard</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_96793">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the motherboard out of the device</step_text>
</owl:NamedIndividual>

