</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_71606">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the screws on the back panel with the #1 Phillips screwdriver bit. The screws in the rubber legs of the burner do not need to be removed. If the back panel has never been opened before, the screws may be more difficult to unscrew.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_71607">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_smart_wrench"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a wrench, remove the bolt from the center of the back panel.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_71608">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The back panel can now be easily lifted off.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_30617">
