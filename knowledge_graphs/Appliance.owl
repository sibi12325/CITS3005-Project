  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the three 7.8 mm Philips #2 screws holding the power cord in place.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_130313">
  <rdf:type rdf:resource="#Step"/>
