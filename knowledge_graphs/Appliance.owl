  <has_step rdf:resource="#Step_50129"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vinotemp VT-12TEDi Motherboard Repair</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vinotemp VT-12TEDi</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">15987</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Vinotemp+VT-12TEDi+Motherboard+Repair/15987</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_50123">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove heat sink fan cable connector. Remove cold sink fan cable connector. Remove temperature control board cable connector. Remove temperature NTC cable connector.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_50124">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove four 12mm Phillips screws. Once the four Phillips screws are removed, the motherboard will dangle there until you remove the heat sink. Be careful.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_50125">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove four 15mm Phillips screws.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_50126">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cut zip tie binding wires together.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_50127">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove two 32mm Phillips screws.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_50128">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">11</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove heat sink slowly to avoid breaking cold sink fan cable. Disconnect cold sink fan cable.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_50129">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">12</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The heat sink and motherboard are removed. Yay! The Vinotemp VT-12TEDi has a very high rate of capacitor failure on the motherboard. Before buying a new motherboard: Check for [http://www.capacitorlab.com/visible-failures/|capacitor failure]. Learn [[Topic:Soldering|how to solder]] and replace the broken capacitor yourself.</step_text>
</owl:NamedIndividual>

