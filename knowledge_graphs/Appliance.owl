</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_96896">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the four screws from the bottom of the processor with a Philips Screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_97261">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the bottom casing of the processor.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_97263">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">If any wires are frayed, cut them off where they are connected by wire nuts.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_97265">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Detach the wire nut from the other connected wires.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_97267">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Replace the bad wire with a new one of the same gauge.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_97272">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Strip the new wire to the same length as the other wires and connect them to a wire nut.</step_text>
</owl:NamedIndividual>

