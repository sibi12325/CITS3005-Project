<owl:NamedIndividual rdf:about="#Step_193709">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Detach the smart watch from the band by pushing on the back of the watch housing. The power button may fall out once the smart watch is detached. Set aside the power button and watch band as they are no longer needed for further disassembly.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_193710">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a plastic opening tool to carefully pry the LCD screen from the watch housing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_193826">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a plastic opening tool or spudger to lift the screen assembly press-fit connector free.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_193828">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Set aside the detached LCD screen to access the battery.</step_text>
</owl:NamedIndividual>

