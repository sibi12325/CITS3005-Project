</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_14146">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_ph00_phillips_screwdriver"/>
  <uses_tool rdf:resource="#Tool_2.5mm_flathead_screwdriver"/>
