<owl:NamedIndividual rdf:about="#Step_157699">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the two black flaps located at the sides of the battery, pull it towards you. Carefully remove the battery.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_157707">
  <rdf:type rdf:resource="#Step"/>
