  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mixer</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">75360</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Tristar+Handmixer+Gears+Replacement/75360</has_url>
</owl:NamedIndividual>

