</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_74591">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place Swiffer SweeperVac on a flat surface.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_74592">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Press the release button.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_74593">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the dirt container out of the device housing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_73158">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the point where the hose connects to the dirt container compartment. Use tweezers, a brush, or a similar tool to dislodge the debris within the hose.</step_text>
</owl:NamedIndividual>

