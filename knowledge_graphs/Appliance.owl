<owl:NamedIndividual rdf:about="#Step_73468">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a phillips 1 screwdriver, remove the 9.5 mm screw holding the circuit board in place. The screw may be hidden underneath the seal.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_73470">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_needlenose_pliers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using pliers, remove the connections for each of the four wires connecting to the circuit board. Remember which color wire corresponds to which connection.  Improper placement will not allow your mixer to turn on.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_73471">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Grab the four wires from the circuit board. Tilt the board back to remove it.</step_text>
</owl:NamedIndividual>

