</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162116">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Press and hold the tabs on both sides of the battery.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162117">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the battery from the Deebot</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162118">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the screw attaching the battery housing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162119">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Inert the new battery casing into the housing. Replace the screw attaching the housing. Replace the batter housing in the Deebot.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_86043">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
