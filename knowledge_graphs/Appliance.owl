  <has_step rdf:resource="#Step_156786"/>
  <has_step rdf:resource="#Step_156707"/>
  <has_step rdf:resource="#Step_156781"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hitachi HB-B102 Automatic Home Bakery  II Outer Casing Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hitachi HB-B102 Automatic Home Bakery  II</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">78425</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Hitachi+HB-B102+Automatic+Home+Bakery++II+Outer+Casing+Replacement/78425</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_78440">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <uses_tool rdf:resource="#Tool_rubber_mallet"/>
  <has_step rdf:resource="#Step_156751"/>
  <has_step rdf:resource="#Step_156754"/>
  <has_step rdf:resource="#Step_156759"/>
  <has_step rdf:resource="#Step_156760"/>
  <has_step rdf:resource="#Step_156767"/>
  <has_step rdf:resource="#Step_156770"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hitachi HB-B102 Automatic Home Bakery  II Seal Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hitachi HB-B102 Automatic Home Bakery  II</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">78440</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Hitachi+HB-B102+Automatic+Home+Bakery++II+Seal+Replacement/78440</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156751">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">First, open up the home bakery and remove the bread pan.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156754">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the paddle/mixer out from inside the bread pan.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156759">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the bread pan over and locate the "C" shaped clip. Orient the clip so the ends line up with the flat part of the bolt. Using tweezers or "C" clip pliers, pry the clip apart and remove it.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156760">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Once the clip is removed, the wing washer and circular washer can be easily removed by hand or tweezer.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156767">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_rubber_mallet"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a rubber mallet or other blunt object to push the bolt until it is flush with the seal.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156770">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the bread pan over and remove the bolt by hand or plyers if necessary. Remove the seal and replace with a new one. Make sure the seal is oriented the correct direction (refer to the picture)</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_78450">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <uses_tool rdf:resource="#Tool_utility_scissors"/>
  <uses_tool rdf:resource="#Tool_wire_cutter"/>
  <has_step rdf:resource="#Step_156795"/>
  <has_step rdf:resource="#Step_156693"/>
  <has_step rdf:resource="#Step_156703"/>
  <has_step rdf:resource="#Step_156696"/>
  <has_step rdf:resource="#Step_156831"/>
  <has_step rdf:resource="#Step_156786"/>
  <has_step rdf:resource="#Step_156707"/>
  <has_step rdf:resource="#Step_156781"/>
  <has_step rdf:resource="#Step_156811"/>
  <has_step rdf:resource="#Step_156813"/>
  <has_step rdf:resource="#Step_156823"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hitachi HB-B102 Automatic Home Bakery  II Motor Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hitachi HB-B102 Automatic Home Bakery  II</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">78450</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Hitachi+HB-B102+Automatic+Home+Bakery++II+Motor+Replacement/78450</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156811">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the capacitor (aka the black box) to the left of the motor). Unscrew the 1cm screw holding the capacitor in place. The screw will stay with the capacitor. Lift the capacitor and place to the side.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156813">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the bread maker on its side so you can access the bottom. The belt will need to be removed. For help on that please see: https://www.ifixit.com/Guide/Hitachi+HB-B102+Automatic+Home+Bakery++II+Belt+Replacement/78424 Unscrew the three 6mm screws holding the motor to the metal plate. Gently pull the motor away from the metal plate.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156823">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_wire_cutter"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Now that you have the motor out, locate the black cable coming out of the motor and linking it to two blue cables. You'll need to cut this wire with scissors or wire cutters. Next, you'll need to wire in your new motor. To do this follow this handy guide: [https://www.youtube.com/watch?v=-WN-lBkjkAc|Wire Stripping and Crimping 101]</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_78455">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <has_step rdf:resource="#Step_156841"/>
  <has_step rdf:resource="#Step_156842"/>
  <has_step rdf:resource="#Step_156843"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hitachi HB-B102 Automatic Home Bakery  II Control Panel Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hitachi HB-B102 Automatic Home Bakery  II</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">78455</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Hitachi+HB-B102+Automatic+Home+Bakery++II+Control+Panel+Replacement/78455</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156841">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Begin by using a # 2 Phillips head screwdriver to remove the two 1.3 cm screws that secure the control panel onto the main body.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156842">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Now gently pry on the top of the control panel with a metal spudger, it should then pop up. Be careful when it is being removed that you do not damage the tabs that hold the bottom of the panel in place.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156843">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Now flip the control panel over so that the wire connections are visible. All of the wires are easily removed by grabbing the metal connector and pulling straight out from the panel.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_78524">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_heavy_duty_spudger"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <has_step rdf:resource="#Step_156955"/>
  <has_step rdf:resource="#Step_156957"/>
  <has_step rdf:resource="#Step_156961"/>
  <has_step rdf:resource="#Step_156964"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hamilton Beach 70740 Gears Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hamilton Beach 70740</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">78524</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Hamilton+Beach+70740+Gears+Replacement/78524</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156955">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Turn the bowl counterclockwise and lift.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156957">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_heavy_duty_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the nylon spudger to remove  the plastic screw covers.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156961">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a #2 Phillips head screwdriver, remove the four 10 mm screws and pull the shaft straight upwards to uncover gears.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156964">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Firmly grasp center piece of gear mechanism and pull straight upward.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_78753">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <uses_tool rdf:resource="#Tool_soldering_station"/>
