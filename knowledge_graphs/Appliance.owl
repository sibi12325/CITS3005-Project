</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_96417">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the tweezers around the battery. Firmly grip the battery with the tweezers. Pull the battery out and replace it with a new 371 Renata watch battery.</step_text>
</owl:NamedIndividual>

