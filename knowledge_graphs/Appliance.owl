  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">AmazonBasics B00Q3KFX8U</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">62156</has_guidid>
