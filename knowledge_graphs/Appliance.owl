<owl:NamedIndividual rdf:about="#Step_41143">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Make sure toaster is unplugged. Turn toaster upside down so screws on bottom side are facing toward the ceiling and the door is facing towards you.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41144">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">On the left hand side, unscrew the middle screw.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41145">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the panel on the left hand side. The indicator light is located in the red tube that has wires covered by black tubing coming out.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41147">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the black wires to remove the indicator light from the red tube.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41148">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use scissors to cut open cord sheathing and peel back.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41149">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the wire-cutters to remove the light bulb.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41150">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Solder the replacement light bulb back onto its original position.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41153">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_electrical_tape_in_6_assorted_colors"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use electrical tape to reseal the sheathing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41186">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reinsert the indicator light back into the red tube.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41154">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Turn toaster upside down so screws on bottom side are facing toward the ceiling and the door is facing towards you.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41155">
