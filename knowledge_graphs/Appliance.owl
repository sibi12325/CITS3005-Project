<owl:NamedIndividual rdf:about="#Step_109040">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pry the battery housing from the top right corner using a spudger. Once loosened, remove the battery housing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109043">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_tweezers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">On the power button, locate a small grey washer on the interior side of the watch. Use a pair of tweezers or another pointed object to push the washer out of its socket, and remove the power button.</step_text>
</owl:NamedIndividual>

