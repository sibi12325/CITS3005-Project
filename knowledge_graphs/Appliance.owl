</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Tool_1.5mm_hex_screwdriver">
  <rdf:type rdf:resource="#Tool"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.5mm hex screwdriver</rdfs:label>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ifixit.com/Tools/1-5mm-Hex-Screwdriver/IF145-025</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_40051">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_ph0_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 12mm black Phillips screws using a Phillips #0 screwdriver. Reminder for reassembly; This bottom piece is first to be screwed back on.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_40052">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_ph0_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 12mm Phillips screws using a Phillips #0 screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_40053">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_1.5mm_hex_screwdriver"/>
  <uses_tool rdf:resource="#Tool_t5_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the grey plastic casing. Remove the two 12mm hexagonal screws using a 1.5mm hex screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_11197">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_1.5mm_hex_screwdriver"/>
  <uses_tool rdf:resource="#Tool_ph0_phillips_screwdriver"/>
  <has_step rdf:resource="#Step_40051"/>
  <has_step rdf:resource="#Step_40052"/>
  <has_step rdf:resource="#Step_40053"/>
  <has_step rdf:resource="#Step_40054"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Breville BKC600XL Water Valve Replacement</has_title>
