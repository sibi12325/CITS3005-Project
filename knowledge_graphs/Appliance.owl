  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Luxury Fashion Quality Military Quartz Watch Link Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Watch</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">83608</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Luxury+Fashion+Quality+Military+Quartz+Watch+Link+Replacement/83608</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162920">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Measure the watch band to decide how many links to remove. It is better to remove an even number of links to keep the clasp centered and symmetrical.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162940">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_pin_punch"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Separate the clasp by pushing in the spring pin.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162921">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_pin_punch"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the clasp from the lug by pushing in on the spring  pin.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162948">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_pin_punch"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">To remove the first link and lug, push in on the link pin to raise it up. Grasp the link pin and pull it out from the link.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_162950">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_pin_punch"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Push in on the second link pin. Grasp the link pin and pull it up and out.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_164419">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the link apart to remove it from the band.</step_text>
</owl:NamedIndividual>

