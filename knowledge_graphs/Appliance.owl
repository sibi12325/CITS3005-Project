</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Tool_end_nippers">
  <rdf:type rdf:resource="#Tool"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">end nippers</rdfs:label>
