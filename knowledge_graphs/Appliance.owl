<owl:NamedIndividual rdf:about="#Step_125412">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the plastic cover from the back of the device.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_125413">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the batteries from their positions within the device.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_125414">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove five black 10 mm Phillips #0 screws from the back side of the device. Remove two silver-colored 5 mm Phillips from the left side of the internal cover.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_125415">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Separate the back cover from the main portion of the device.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_125416">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Desolder the speaker wires from the motherboard.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_56854">
  <rdf:type rdf:resource="#Guide"/>
