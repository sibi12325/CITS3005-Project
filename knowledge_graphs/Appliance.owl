  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Neato Botvac D75 Motherboard Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Neato Botvac D75</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">79197</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Neato+Botvac+D75+Motherboard+Replacement/79197</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_158608">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">14</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Carefully remove the plugs connected to top side of the motherboard. Be careful to not pull the white casings that attach the plugs to the board.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_161432">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">15</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the cables attached to the under side of the motherboard. Similarly to the previous step, be careful as to not remove the casing attaching the cable to the board.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_79564">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_torx_bits_with_handle"/>
  <uses_tool rdf:resource="#Tool_flat_blade_screwdriver"/>
