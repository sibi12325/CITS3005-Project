  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Separate the zip-tie holding the wires to the motor.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_168466">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Now the switch is separated from the motor and blender frame.</step_text>
</owl:NamedIndividual>

