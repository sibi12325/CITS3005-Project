  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Black and Decker BL3000S Motor Fan Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Black and Decker BL3000S</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">11396</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Black+and+Decker+BL3000S+Motor+Fan+Replacement/11396</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_40766">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_needlenose_pliers"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using pliers, remove the top nut from the fan and remove the washer. Use a small towel to hold the fan in place. This makes taking off the nut easier to do.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_42433">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the fan blades from the blender base.</step_text>
</owl:NamedIndividual>

