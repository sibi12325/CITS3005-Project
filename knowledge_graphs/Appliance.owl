  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Press the release button for the Time Setting Dial using a thin pin.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_96249">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the dial out gently with fingers.</step_text>
</owl:NamedIndividual>

