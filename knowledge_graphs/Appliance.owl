<owl:NamedIndividual rdf:about="#Step_130678">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_solder"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">12</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the switch by unsoldering the 8 solder points on the motherboard. To reinstall, solder the switch  back into the same location on the motherboard.</step_text>
</owl:NamedIndividual>

