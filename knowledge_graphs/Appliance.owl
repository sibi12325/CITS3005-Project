  <has_step rdf:resource="#Step_95150"/>
  <has_step rdf:resource="#Step_95152"/>
  <has_step rdf:resource="#Step_95153"/>
  <has_step rdf:resource="#Step_95155"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Comfort Zone CZ6C Power Cable Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Comfort Zone CZ6C</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">41232</has_guidid>
