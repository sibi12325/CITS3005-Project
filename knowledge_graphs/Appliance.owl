  <has_step rdf:resource="#Step_59589"/>
  <has_step rdf:resource="#Step_59590"/>
  <has_step rdf:resource="#Step_59586"/>
  <has_step rdf:resource="#Step_59587"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Brother XR-40 Lightbulb Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Brother XR-40</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">22759</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Brother+XR-40+Lightbulb+Replacement/22759</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59538">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the machine on a solid surface. Remove the 14mm Phillips #2 screw holding on the face cover.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59516">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 14mm Phillips #2 screws and two 22mm Phillips #2 screws that secure the rear casing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59520">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lay the machine on its side. Remove the four 10mm Phillips #2 screws that hold the bottom panel in place. The feet will come off with the screws.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59537">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the bottom panel.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59521">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the machine upright. Remove the rear casing. You may have to wiggle the panel and pull at the same time.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59585">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_utility_scissors"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using scissors, clip the two zip ties securing the lightbulb wire.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59589">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the machine on its side, with access to the bottom. Gently pull out the white wire connecting the bulb to the machine.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59590">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Move the wire out from under the plastic hook.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59586">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_flathead_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the machine upright. Using the flathead screwdriver, remove the 5mm Flathead screw holding on the lightbulb bracket.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59587">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the lightbulb assembly.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_22778">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
