</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_130348">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Slide the cooling vent out from the silver plastic part of the blender base and replace it.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_62061">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <uses_tool rdf:resource="#Tool_t9_torx_screwdriver"/>
