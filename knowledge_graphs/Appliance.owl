</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180437">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_ph0_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the phillip's screws from the motherboard.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180439">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the motherboard over.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180441">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove any plugs from  the motherboard.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_180443">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Desolder wires and cords. Replace old motherboard with new one, and resolder wires and cords.</step_text>
</owl:NamedIndividual>

