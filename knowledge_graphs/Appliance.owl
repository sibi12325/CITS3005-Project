</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165618">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove 2 hose clamps from heating element. Remove heating element from tubes.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_163496">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
