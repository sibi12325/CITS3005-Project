  <has_step rdf:resource="#Step_183544"/>
  <has_step rdf:resource="#Step_183545"/>
  <has_step rdf:resource="#Step_183550"/>
  <has_step rdf:resource="#Step_184317"/>
  <has_step rdf:resource="#Step_184315"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vornado Duo Circuit Board Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vornado Duo</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">99077</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Vornado+Duo+Circuit+Board+Replacement/99077</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_183542">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Start off by using a Philips #2 screwdriver to remove all 8 10mm exterior screws on the rear of the device.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_183544">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Carefully take front panel off.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_183545">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Loosen 5 mm screw at bottom of fan blade located inside the blades.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_183550">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_2_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove fan blade after unscrewing top 2 screws located on each side of the covered board.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_184317">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
