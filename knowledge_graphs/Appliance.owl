  <has_step rdf:resource="#Step_180561"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Shark NV352 Motherboard Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Shark NV352</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">98290</has_guidid>
