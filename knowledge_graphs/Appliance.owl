</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_133158">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the Water Reservoir Cap and the Water Reservoir from the brewer by simply pulling up and out</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_133160">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the three 9.5 mm Phillips #1 screws that secure the small circular screen to the bottom of the water reservoir.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_133161">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This circular screen has a rubber "O"-ring around the base.  Use the plastic opening tool to pry off the rubber seal.  Replace with undamaged seal.</step_text>
</owl:NamedIndividual>

