  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Shark Rotator Powered Lift-Away Brush Belt Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Shark Rotator Powered Lift-Away</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">98553</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Shark+Rotator+Powered+Lift-Away+Brush+Belt+Replacement/98553</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181064">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Push lift away button and pull the handle up to separate the body from the wand section.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181069">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Push lift away button on the wand then lift up on the top section to separate.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181072">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tilt the wand shaft backwards for convenient.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181073">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lay the head nozzle piece upside down. Locate and rotate the three gray taps to the unlock position.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181074">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">With your fingers, pry and pull the brush cage open.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181077">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t10_torx_screwdriver"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the 13 screws (circled in yellow) using a phillips screwdriver. Be sure to keep track of which screws go into which hole. Remove the one screw (circled in red) that has a Torx head. The Torx screw is hidden under a rubber gasket. Carefully, lift up the gasket without damaging it to access this screw.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_182138">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift up the top cover now that all of the screws have been removed.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181131">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pry tab out with a flat head screwdriver or any wedge-like tool.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181132">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gently press in on brush to detach. Pull brush down and out.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181133">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Slide the belt off of the gear.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_98554">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_jis_1_screwdriver"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <has_step rdf:resource="#Step_181258"/>
  <has_step rdf:resource="#Step_181259"/>
  <has_step rdf:resource="#Step_181260"/>
  <has_step rdf:resource="#Step_181323"/>
  <has_step rdf:resource="#Step_181327"/>
  <has_step rdf:resource="#Step_181328"/>
  <has_step rdf:resource="#Step_181370"/>
  <has_step rdf:resource="#Step_181371"/>
  <has_step rdf:resource="#Step_181134"/>
  <has_step rdf:resource="#Step_181135"/>
  <has_step rdf:resource="#Step_181236"/>
  <has_step rdf:resource="#Step_181286"/>
  <has_title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Shark Navigator Lightweight Upright Motor Replacement</has_title>
  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Shark Navigator Lightweight Upright</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">98554</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/Shark+Navigator+Lightweight+Upright+Motor+Replacement/98554</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181258">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Detach the hose connected on the bottom of the vacuum by pulling it out of the casing.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181259">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lay the vacuum on its side. Press in the black, rubberish button located on the back of the vacuum to detach the vacuum head from the body.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181260">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip over the vacuum head so the brush head is facing up.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181323">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the small front wheels with a metal spudger by inserting the metal spudger in front of  the wheel and slowly prying up. This will free the wheel and reveal the screw beneath it. Repeat this for the other wheel.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181327">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_jis_1_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 18mm screws with a JIS 1 screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181328">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_jis_1_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the four 18mm screws located on the top corners and the bottom/middle of the vacuum head using a JIS 1 screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181370">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_jis_1_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 18mm screws located on the inside of the large wheels using a JIS 1 screwdriver.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181371">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the plastic cover by prying the plastic cover up using a plastic opening tool. Prying in multiple locations may be necessary to create separation.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181134">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the motor on the top right corner of the vacuum head shell.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181135">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the belt from the motor gear by sliding the belt to the right.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181236">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the white metal wire connector from the motor. The white cable is positive.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_181286">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">11</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the black metal wire connector from the motor. The black cable is negative.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Guide_98555">
  <rdf:type rdf:resource="#Guide"/>
  <uses_tool rdf:resource="#Tool_jis_1_screwdriver"/>
  <uses_tool rdf:resource="#Tool_opening_tool"/>
  <uses_tool rdf:resource="#Tool_metal_spudger"/>
