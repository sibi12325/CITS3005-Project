<owl:NamedIndividual rdf:about="#Step_125315">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the four wires 2 orange, 1 black, and 1 white. It will be important to remember which ports the wires are plugged into. The clips on the wires may be sharp. You can pull them out with a pair of blunt ESD tweezers.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_125316">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t15_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the two screws on both sides of the metal bridge with a Torx T-15.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_125317">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the green wire away from the motor housing after removing the screw.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_125319">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Separate the clip from the green wire.</step_text>
</owl:NamedIndividual>

