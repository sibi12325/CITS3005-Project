</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_113119">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_wire_cutter"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the two wires from the motor platform.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_113173">
  <rdf:type rdf:resource="#Step"/>
