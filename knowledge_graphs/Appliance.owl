<owl:NamedIndividual rdf:about="#Step_100746">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Die Aufschäumdüse abschrauben. Dafür kann ein Engländer verwendet werden.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100747">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Den Kaffeebohnenbehälter entriegeln und abnehmen.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100748">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Die fünf PH1 Schrauben an der linken und rechten Seite lösen. Achtung 220 Volt! Unbedingt Stecker vorher rausziehen! Maschine vor dem Öffnen abkühlen lassen!</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100749">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Den rechten Schalter durch lösen der Schaltermutter ausbauen. Mit einer kleinen Zange die Kontermutter festhalten. Achtung: Dabei nicht das Gehäuse verkratzen!</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100750">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Das Umschaltventil sitzt von oben gesehen an der rechten Gehäusewand. Mutter zur Brühgruppe öffnen. Falls sie festsitzt mit Essigtinktur behandeln und einwirken lassen. Schlauch zum Siebträger abschrauben. Ablaufschlauch abziehen.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100751">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_t25_torx_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Die Befestigungsschraube mit einem T25 Torxschraubendreher entfernen.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100753">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Die Halterung vom Umschlaltventil lösen.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100755">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Die Mutter für den Ablaufschlauch lösen Die Dichtung in der Mutter prüfen und ggf. ersetzen. Dichtung mit Siliconfett einfetten.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100754">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Durch das Drehen der Dampfdüse wird das Ventil betätigt. Je nach Stellung der Düse wird das Wasser bzw. der Dampf durch die Düse oder zum Siebträger geleitet.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100756">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dampfdüse abschrauben. Dichtungen an der Dampfdüse prüfen und fetten. Beim Einbau darauf achten, dass der Stift am Ende der Düse in die Rille der Ventilspindel greift. Funktionsweise des Umschaltventils durch Drehen der Düse prüfen.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100757">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Die Ventilspindel kann jetzt seitlich aus dem Ventil gezogen werden.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_100758">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">11</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dichtungen überprüfen und ggf. ersetzen. Dichtungen vor dem Einbau mit Siliconfett einfetten.</step_text>
</owl:NamedIndividual>

