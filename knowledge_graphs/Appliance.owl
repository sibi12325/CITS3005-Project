  <has_category rdf:datatype="http://www.w3.org/2001/XMLSchema#string">iRobot Roomba 595 Pet Series</has_category>
  <has_guidid rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">99930</has_guidid>
  <has_url rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Guide/iRobot+Roomba+595+Pet+Series+Main+Brush+Replacement/99930</has_url>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185502">
  <rdf:type rdf:resource="#Step"/>
  <uses_tool rdf:resource="#Tool_phillips_screwdriver"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the four 10mm screws with a Phillips #1 screwdriver. Gently lift off the external covering for the Roomba's underside.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185509">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Press lightly on the yellow tabs and lift the cage cover containing the brush.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_185514">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the brush from the right side and slide it from its container within the Roomba.</step_text>
</owl:NamedIndividual>

