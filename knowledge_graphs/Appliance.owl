<owl:NamedIndividual rdf:about="#Step_143834">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the black tray for an easier disassembly</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_143836">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Move the steam nozzle to the side to have it out of the way.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_143837">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the flat head screw in the center of the filter hodler.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_143838">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the screen.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_143839">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the gasket.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_143842">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Now we have all the parts removed: the screen, the gasket, the hollow screw, the spring and the valve. Remove the pin first.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_143843">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the spring.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_143844">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the metal plate.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_143845">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">You are left with the screen and and the hollow screw.</step_text>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_143848">
  <rdf:type rdf:resource="#Step"/>
  <step_order rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</step_order>
  <step_text rdf:datatype="http://www.w3.org/2001/XMLSchema#string">All parts can be cleaned individually now. It is advisable to also clean the socket of the filter on the machine. Reassemble the parts in reverse order after cleaning. Enjoy your coffee!</step_text>
</owl:NamedIndividual>

